		<Item class="Script" referent="RBX7">
			<Properties>
				<bool name="Archivable">true</bool>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">CooldownScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Dagger = Tool.Handle

local vCharacter 

local cooldown = Tool:FindFirstChild(&quot;Cooldown&quot;)
while cooldown == nil do 
&#9;wait()
&#9;cooldown = Tool:FindFirstChild(&quot;Cooldown&quot;)
end

function cd()
&#9;cooldown = Tool:FindFirstChild(&quot;Cooldown&quot;)
&#9;local firePart = Dagger:FindFirstChild(&quot;FirePart&quot;)
&#9;local fire
&#9;if firePart then fire = firePart:FindFirstChild(&quot;Fire&quot;) end&#9;
&#9;if cooldown and cooldown.Value == 1 then &#9;&#9;&#9;&#9;
&#9;&#9;wait(25.0)&#9;
&#9;&#9;cooldown.Value = 0&#9;&#9;
&#9;&#9;if fire then fire.Enabled = true end
&#9;end
end

cooldown.Changed:connect(cd)

function onEquipped()
&#9;vCharacter = Tool.Parent
end

Tool.Equipped:connect(onEquipped)
</ProtectedString>
