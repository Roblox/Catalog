		<Item class="Script" referent="RBX8">
			<Properties>
				<bool name="Archivable">true</bool>
				<bool name="Disabled">true</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">DoomScript</string>
				<ProtectedString name="Source">local vCharacter  = script.Parent
local myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
local myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)

local ch = vCharacter:GetChildren()

local bodyVelocity

local firePart = Instance.new(&quot;Part&quot;)
firePart.FormFactor = 3
firePart.Size = Vector3.new(0.2, 0.2, 0.2)
firePart.Transparency = 1.0
firePart.Name = &quot;FirePart&quot;
firePart.CanCollide = false

local debris = game:GetService(&quot;Debris&quot;)

local fire = Instance.new(&quot;Fire&quot;)
fire.Parent = firePart 
fire.Color = Color3.new(8/255, 8/255, 8/255)
fire.SecondaryColor = Color3.new(102/255, 255/255, 153/255)
fire.Heat = 25
fire.Size = 6

local targetTorso = script:FindFirstChild(&quot;Torso&quot;)
while targetTorso == nil do 
&#9;targetTorso = script:FindFirstChild(&quot;Torso&quot;)
&#9;wait()
end

local storeHealth = 0
local storeCFrame

for i = 1, #ch do 
&#9;if ch[i].className == &quot;Tool&quot; then 
&#9;&#9;ch[i]:Remove()
&#9;end
end

if myHumanoid then 
&#9;myHumanoid.WalkSpeed = 0.0
end

if myTorso then 
&#9;bodyVelocity = Instance.new(&quot;BodyVelocity&quot;)
&#9;bodyVelocity.P = 300000
&#9;bodyVelocity.maxForce = Vector3.new(bodyVelocity.P, bodyVelocity.P, bodyVelocity.P)
&#9;bodyVelocity.velocity = Vector3.new(0, 3, 0)
&#9;bodyVelocity.Parent = myTorso

&#9;firePart.Parent = game.Workspace
&#9;
&#9;local fireWeld = Instance.new(&quot;Weld&quot;)
&#9;fireWeld.Name = &quot;FireWeld&quot;
&#9;fireWeld.Part0 = myTorso
&#9;fireWeld.Part1 = firePart
&#9;fireWeld.Parent = myTorso
end

wait(2)
if myHumanoid then 
&#9;storeHealth = myHumanoid.Health 
&#9;storeCFrame = myTorso.CFrame
&#9;myHumanoid.Health = 0 &#9;
end 
if bodyVelocity then bodyVelocity:Remove() end

wait(1.0)

function follow(targetTorso)
&#9;while targetTorso and firePart do 
&#9;&#9;local distance = (targetTorso.Position - firePart.Position).magnitude
&#9;&#9;if distance &gt; 2 then
&#9;&#9;&#9;if firePart then firePart.Velocity = (targetTorso.Position - firePart.Position).unit * 30.0  end
&#9;&#9;end
&#9;&#9;wait()
&#9;end
end

if targetTorso and targetTorso.Value then&#9;
&#9;targetTorso = targetTorso.Value

&#9;if fireWeld then fireWeld:Remove() end

&#9;local bodyForce = Instance.new(&quot;BodyForce&quot;)
&#9;bodyForce.force = Vector3.new(0, firePart:GetMass() * 196.1, 0)
&#9;bodyForce.Parent = firePart

&#9;firePart.Velocity = Vector3.new(0.0, 0.0, 0.0)&#9;
&#9;firePart.CFrame = storeCFrame

&#9;coroutine.resume(coroutine.create(function() follow(targetTorso) end))&#9;

&#9;debris:AddItem(firePart, 6)

&#9;firePart.Touched:connect(function(hit) &#9;&#9;
&#9;&#9;if firePart and hit and hit.Parent and hit.Parent:FindFirstChild(&quot;Torso&quot;) and hit.Parent.Torso == targetTorso then 
&#9;&#9;&#9;local humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)&#9;&#9;&#9;
&#9;&#9;&#9;if humanoid and firePart then 
&#9;&#9;&#9;&#9;if firePart then firePart:Remove() firePart = nil end
&#9;&#9;&#9;&#9;if (humanoid.Health + (storeHealth /2) + 10)&lt; humanoid.MaxHealth then 
&#9;&#9;&#9;&#9;&#9;humanoid.Health = humanoid.Health + (storeHealth/2) + 10
&#9;&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;&#9;humanoid.Health = humanoid.MaxHealth
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end)
else &#9;
&#9;if firePart then firePart:Remove() end
&#9;if fireWeld then fireWeld:Remove() end
end


wait(6.0)
script:Remove()
</ProtectedString>
