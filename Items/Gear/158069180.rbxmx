&#9;end
end

function Lunge()
&#9;if not MiniDebounce and not SpinDebounce then
&#9;&#9;MiniDebounce = true
&#9;&#9;LungeSound:Play()
&#9;&#9;Damage = Lunge_Damage
&#9;&#9;local NewAttack = Instance.new(&quot;StringValue&quot;)
