&#9;&#9;local Time = RunService.Stepped:wait()
&#9;&#9;if (Time - Last_Attack) &lt; 0.2 then
&#9;&#9;&#9;Lunge()
&#9;&#9;else
&#9;&#9;&#9;Attack()
&#9;&#9;end
&#9;&#9;Last_Attack = Time
&#9;&#9;Tool.Enabled = true
&#9;end
end

