&#9;LeftSwordWeld.C1 = CFrame.new(0, 0, -2, 0, -1, -0, -1, 0, -0, 0, 0, -1)
&#9;
end

function Attack()
&#9;if not SpinDebounce then
&#9;&#9;Damage = Slash_Damage
&#9;&#9;SlashSound:Play()
&#9;&#9;local Anim = Instance.new(&quot;StringValue&quot;)
&#9;&#9;Anim.Name = &quot;toolanim&quot;
