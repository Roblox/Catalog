&#9;&#9;&#9;&#9;&#9;&#9;local Hit, EndPosition = RayCast(HitPosition, Vector3.new(0, -1, 0), 6, IgnoreObjects)
&#9;&#9;&#9;&#9;&#9;&#9;if Hit then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;local NewPoisonPuddle = BasePoisonPuddle:Clone()
