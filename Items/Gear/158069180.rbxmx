&#9;&#9;MiniDebounce = false
&#9;end
end

function KeyDown(Key)
&#9;Key = string.lower(Key)
&#9;if Key == &quot;e&quot; or Key == &quot;q&quot; or Key == &quot;z&quot; or Key == &quot;x&quot; then
&#9;&#9;if MiniDebounce then
&#9;&#9;&#9;Spin()
&#9;&#9;end
&#9;end
end

function Activated()
&#9;if Tool.Enabled then
