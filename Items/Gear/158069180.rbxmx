&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;wait()
&#9;&#9;end
&#9;end)
&#9;Debris:AddItem(PoisonPuddlesModel, 60)
&#9;PoisonPuddlesModel.Parent = game:GetService(&quot;Workspace&quot;)
&#9;
&#9;local BasePoisonPuddle = BasePart:Clone()
