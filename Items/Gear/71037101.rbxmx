				<string name="Name">TimeDaggerScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Dagger = Tool.Handle

local vCharacter
local myTorso
local myHumanoid 
local vPlayer

local timeWaveSound

local cloneDagger

local AOE = Vector3.new(15, 15, 15)

local damage = 15

local maxDistance = 45.0

local awayFromCharacter = false 

local anchoredParts = {}
local partsToSlow = {}

local timeWave = Instance.new(&quot;Part&quot;) 
timeWave.FormFactor = 3
timeWave.Size = Vector3.new(1, 0.2, 1)
timeWave.Transparency = 1.0
timeWave.CanCollide = false 

local decal = Instance.new(&quot;Decal&quot;)
decal.Specular = 20
decal.Face = 1
decal.Texture = &quot;http://www.roblox.com/asset/?version=1&amp;id=70991028&quot;
decal.Parent = timeWave

local bottomDecal = decal:Clone()
bottomDecal.Specular = 20
bottomDecal.Face = 4
bottomDecal.Parent = timeWave

local debris = game:GetService(&quot;Debris&quot;)

while Tool:FindFirstChild(&quot;StopTime&quot;) == nil do 
&#9;wait()
end

while Tool:FindFirstChild(&quot;SlowTime&quot;) == nil do 
&#9;wait()
end

function tagHumanoid(humanoid, player)
&#9;if humanoid ~= nil then 
&#9;&#9;local creatorTag = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;creatorTag.Value = player 
&#9;&#9;creatorTag.Name = &quot;creator&quot;
&#9;&#9;creatorTag.Parent = humanoid
&#9;&#9;debris:AddItem(creator, 1)
&#9;end
end

function sleep(hit)
&#9;if hit and hit.Parent then 
&#9;&#9;local humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;if hit.Parent.className == &quot;Hat&quot; then 
&#9;&#9;&#9;humanoid = hit.Parent.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;end
&#9;&#9;if humanoid and myHumanoid and humanoid ~= myHumanoid then
&#9;&#9;&#9;tagHumanoid(humanoid, vPlayer)
&#9;&#9;&#9;humanoid:TakeDamage(damage)
&#9;&#9;&#9;local character = humanoid.Parent 
&#9;&#9;&#9;if character and character:FindFirstChild(&quot;SleepScript&quot;) == nil then 
&#9;&#9;&#9;&#9;local cloneScript = Tool.SleepScript:Clone()
&#9;&#9;&#9;&#9;cloneScript.Parent = character 
&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;local sleepy = cloneScript:FindFirstChild(&quot;Sleepy&quot;)
&#9;&#9;&#9;&#9;if sleepy then 
&#9;&#9;&#9;&#9;&#9;sleepy.Value = 4 
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;&#9;cloneScript.Disabled = false 
&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;local sleepy = character.SleepScript:FindFirstChild(&quot;Sleepy&quot;)
&#9;&#9;&#9;&#9;if sleepy then 
&#9;&#9;&#9;&#9;&#9;sleepy.Value = sleepy.Value + 2
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;awayFromCharacter = false
&#9;&#9;end
&#9;end
end

local stopTime = Tool.StopTime
stopTime.Changed:connect(function() 
&#9;if stopTime and stopTime.Value == true and myTorso and myHumanoid then&#9;
&#9;&#9;myHumanoid.WalkSpeed = 0.0
&#9;&#9;local timeWaveDuration = 1.0 
&#9;&#9;local timeWaveClone = timeWave:Clone() 
&#9;&#9;timeWaveClone.Parent = game.Workspace 
&#9;&#9;timeWaveClone.Anchored = true 
&#9;&#9;timeWaveClone.CFrame = myTorso.CFrame 
&#9;&#9;
&#9;&#9;timeWaveSound = Dagger:FindFirstChild(&quot;TimeWaveSound&quot;)
&#9;&#9;if timeWaveSound then timeWaveSound:Play() end 
&#9;&#9;while timeWaveDuration &gt; 0 do 
&#9;&#9;&#9;timeWaveClone.Size = Vector3.new(timeWaveClone.Size.X + 1.5, 0.2, timeWaveClone.Size.Z + 1.5)
&#9;&#9;&#9;timeWaveClone.CFrame = myTorso.CFrame
&#9;&#9;&#9;timeWaveDuration = timeWaveDuration - wait()
&#9;&#9;&#9;wait()
&#9;&#9;end 
&#9;&#9;if timeWaveClone then timeWaveClone:Remove() end
&#9;&#9;myHumanoid.WalkSpeed = 20.0
&#9;&#9;local parts = game.Workspace:FindPartsInRegion3(Region3.new(myTorso.Position - AOE, myTorso.Position +  AOE), vCharacter, 100)
&#9;&#9;for i = 1, #parts do 
&#9;&#9;&#9;
&#9;&#9;&#9;if parts[i].Parent:FindFirstChild(&quot;Humanoid&quot;) then -- treat this as special case
&#9;&#9;&#9;&#9;local torso = parts[i].Parent:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;&#9;local humanoid = parts[i].Parent.Humanoid  
&#9;&#9;&#9;&#9;local character = parts[i].Parent 
&#9;&#9;&#9;&#9;if character:FindFirstChild(&quot;StopTimeScript&quot;) == nil then 
&#9;&#9;&#9;&#9;&#9;local timeScript = Tool:FindFirstChild(&quot;StopTimeScript&quot;):Clone()
&#9;&#9;&#9;&#9;&#9;timeScript.Parent = character 
&#9;&#9;&#9;&#9;&#9;timeScript.Disabled = false 
&#9;&#9;&#9;&#9;end &#9;&#9;&#9;&#9;
&#9;&#9;&#9;elseif parts[i].Anchored == false then  
&#9;&#9;&#9;&#9;table.insert(anchoredParts, parts[i])
&#9;&#9;&#9;&#9;parts[i].Anchored = true 
&#9;&#9;&#9;end 
&#9;&#9;end 

&#9;&#9;local players = game.Players:GetChildren()
&#9;&#9;&#9;for i = 1, #players do
