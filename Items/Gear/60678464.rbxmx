				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">PlaySoundScript</string>
				<ProtectedString name="Source">-- This is not very optimal

local Tool = script.Parent
local Guitar = Tool.Handle

local debris = game:GetService(&quot;Debris&quot;)

local s3 = Guitar:FindFirstChild(&quot;S3&quot;)
while s3 == nil do 
&#9;wait()
&#9;s3 = Guitar:FindFirstChild(&quot;S3&quot;)
end

local mySounds = {Guitar:FindFirstChild(&quot;S1&quot;), Guitar:FindFirstChild(&quot;S2&quot;), Guitar:FindFirstChild(&quot;S3&quot;)}

local playSound = Tool:FindFirstChild(&quot;PlaySound&quot;)
while playSound == nil do 
&#9;playSound = Tool:FindFirstChild(&quot;PlaySound&quot;)
&#9;wait()
end

function playMusic()&#9;
&#9;playSound = Tool:FindFirstChild(&quot;PlaySound&quot;)
&#9;if playSound and playSound.Value == 0 then return end
&#9;if playSound and mySounds then 
&#9;&#9;print(playSound.Value)
&#9;&#9;print(mySounds[playSound.Value])
&#9;&#9;mySounds[playSound.Value]:Play() 
&#9;end&#9;
end

playSound.Changed:connect(playMusic)

local dance = Tool:FindFirstChild(&quot;Dance&quot;) -- Cooldown for Dance ability 
while dance == nil do 
&#9;dance = Tool:FindFirstChild(&quot;Dance&quot;)
&#9;wait()
end

local textures = {60667024, 60667032, 60667053, 60667087}

function waitCD()
&#9;local vCharacter = Tool.Parent
&#9;local myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;if myTorso == nil then 
&#9;&#9;return 
&#9;end
&#9;dance = Tool:FindFirstChild(&quot;Dance&quot;)
&#9;if dance then 
&#9;&#9;if dance.Value == 13 then 
&#9;&#9;&#9;local s4 = Guitar:FindFirstChild(&quot;S4&quot;)
&#9;&#9;&#9;if s4 then 
&#9;&#9;&#9;&#9;s4:Play()
&#9;&#9;&#9;end

&#9;&#9;&#9;for i = 1, 12 do
&#9;&#9;&#9;&#9;tick()
&#9;&#9;&#9;&#9;local part = Instance.new(&quot;Part&quot;)&#9;&#9;&#9;
&#9;&#9;&#9;&#9;part.Shape = &quot;Block&quot;
&#9;&#9;&#9;&#9;part.FormFactor = 3
&#9;&#9;&#9;&#9;part.Size = Vector3.new(1, 1, 1)
&#9;&#9;&#9;&#9;part.Parent = game.Workspace
&#9;&#9;&#9;&#9;part.Name = &quot;Note&quot;
&#9;&#9;&#9;&#9;part.CanCollide = false 

&#9;&#9;&#9;&#9;local partMesh = Instance.new(&quot;SpecialMesh&quot;)
&#9;&#9;&#9;&#9;partMesh.MeshId = &quot;http://www.roblox.com/asset/?id=60666966&quot;
&#9;&#9;&#9;&#9;partMesh.TextureId = &quot;http://www.roblox.com/asset/?id=&quot; .. textures[math.random(1, #textures)]
&#9;&#9;&#9;&#9;partMesh.Parent = part

&#9;&#9;&#9;&#9;local ff = Instance.new(&quot;BodyForce&quot;)
&#9;&#9;&#9;&#9;ff.force = Vector3.new(0, part:GetMass() * 196.1, 0)
&#9;&#9;&#9;&#9;ff.Parent = part
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;local pos = CFrame.Angles(-math.pi/12, i * math.pi/6, 0) * myTorso.CFrame.lookVector
&#9;&#9;&#9;&#9;local spawnPos = myTorso.Position + (pos * 5.0)
&#9;&#9;
&#9;&#9;&#9;&#9;part.CFrame = CFrame.new(spawnPos, spawnPos + pos) 
&#9;&#9;&#9;&#9;part.Velocity = pos * 4.0
&#9;&#9;&#9;&#9;debris:AddItem(part, 2)
&#9;&#9;&#9;end

&#9;&#9;&#9;local parts = game.Workspace:FindPartsInRegion3(Region3.new(myTorso.Position - Vector3.new(10, 10, 10), myTorso.Position + Vector3.new(10, 10, 10)), vCharacter,  100)
&#9;&#9;&#9;for i = 1, #parts do 
&#9;&#9;&#9;&#9;local char = parts[i].Parent
&#9;&#9;&#9;&#9;if char then
&#9;&#9;&#9;&#9;&#9;local humanoid = parts[i].Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;&#9;&#9;if humanoid then 
&#9;&#9;&#9;&#9;&#9;&#9;local newScript = char:FindFirstChild(&quot;GuitarDanceScript&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;if newScript == nil then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript = script.GuitarDanceScript:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript.Parent = char
&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript.Disabled = false
&#9;&#9;&#9;&#9;&#9;&#9;end&#9;&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;if dance.Value &lt;= 0 then 
&#9;&#9;&#9;dance.Value = 0
&#9;&#9;&#9;return
&#9;&#9;else 
&#9;&#9;&#9;wait(1.0)
&#9;&#9;&#9;dance.Value = dance.Value - 1&#9;&#9;&#9;
&#9;&#9;end&#9;&#9;
&#9;end
end

dance.Changed:connect(waitCD)






