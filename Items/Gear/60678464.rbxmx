				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">GuitarScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Guitar = Tool.Handle

local vCharacter 

local playSound

local guitarAnim
local guitarSound 

local guitarClone 
local stunGuitarClone
local dance

Tool.Enabled = true 

function stunThemAll(key)
&#9;local stunGuitarAnim
&#9;if key:lower() == &quot;q&quot; then 
&#9;&#9;dance = Tool:FindFirstChild(&quot;Dance&quot;)
&#9;&#9;if dance and dance.Value &lt;= 0 then &#9;&#9;&#9;
&#9;&#9;&#9;if guitarClone == nil or guitarClone.Parent == nil then
&#9;&#9;&#9;&#9;dance.Value = 13&#9;&#9;&#9;
&#9;&#9;&#9;&#9;print(&quot;Entering the loop&quot;)
&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;stunGuitarAnim = myHumanoid:LoadAnimation(Tool.GuitarAnim)
&#9;&#9;&#9;&#9;if stunGuitarAnim then stunGuitarAnim:Play() end

&#9;&#9;&#9;&#9;stunGuitarClone = Guitar:Clone()
&#9;&#9;&#9;&#9;stunGuitarClone.Parent = vCharacter
&#9;&#9;&#9;&#9;stunGuitarClone.Name = &quot;StunGuitar&quot;
&#9;&#9;&#9;&#9;Guitar.Transparency = 1.0

&#9;&#9;&#9;&#9;guitarWeld = Instance.new(&quot;Weld&quot;)
&#9;&#9;&#9;&#9;guitarWeld.Part0 = vCharacter.Torso
&#9;&#9;&#9;&#9;guitarWeld.Part1 = stunGuitarClone
&#9;&#9;&#9;&#9;guitarWeld.Name = &quot;HoldGuitar&quot;
&#9;&#9;&#9;&#9;guitarWeld.Parent = stunGuitarClone
&#9;&#9;&#9;&#9;guitarWeld.C1 = CFrame.Angles(math.pi, 0, -math.pi/4) * CFrame.new(Vector3.new(0, -0.2, 0.9))&#9;&#9;
&#9;
&#9;&#9;&#9;&#9;wait(2.0)
&#9;&#9;&#9;&#9;Guitar.Transparency = 0.0
&#9;&#9;&#9;&#9;if stunGuitarClone then stunGuitarClone:Remove() end
&#9;&#9;&#9;&#9;if stunGuitarAnim then stunGuitarAnim:Stop() end
&#9;&#9;&#9;&#9;
&#9;&#9;&#9;end&#9;&#9;&#9;
&#9;&#9;end&#9;&#9;
&#9;end
end

function onActivated()
&#9;if Tool.Enabled == false then 
&#9;&#9;return
&#9;end
&#9;Tool.Enabled = false

&#9;vCharacter = Tool.Parent
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;
&#9;if myHumanoid == nil then
&#9;&#9;return
&#9;end

&#9;local dance = Tool:FindFirstChild(&quot;Dance&quot;)
&#9;if dance and dance.Value &gt; 11 then return end

&#9;guitarClone = Guitar:Clone()
&#9;guitarClone.Parent = vCharacter
&#9;guitarClone.Name = &quot;Guitar&quot;
&#9;Guitar.Transparency = 1.0

&#9;guitarWeld = Instance.new(&quot;Weld&quot;)
&#9;guitarWeld.Part0 = vCharacter.Torso
&#9;guitarWeld.Part1 = guitarClone
&#9;guitarWeld.Name = &quot;HoldGuitar&quot;
&#9;guitarWeld.Parent = guitarClone
&#9;guitarWeld.C1 = CFrame.Angles(math.pi, 0, -math.pi/4) * CFrame.new(Vector3.new(0, -0.2, 0.9))

&#9;guitarAnim = myHumanoid:LoadAnimation(Tool.GuitarAnim)
&#9;if guitarAnim then guitarAnim:Play() end&#9;
&#9;tick() -- random seeding &#9;&#9;
&#9;playSound = Tool:FindFirstChild(&quot;PlaySound&quot;)
&#9;if playSound then playSound.Value = math.random(1, 3) end
&#9;wait(10)
&#9;if playSound then playSound.Value = 0 end
&#9;wait(1.0)
&#9;Guitar.Transparency = 0.0
&#9;if guitarClone then guitarClone:Remove() end
&#9;Tool.Enabled = true&#9;
&#9;
end

function onEquipped(mouse)
&#9;vCharacter = Tool.Parent
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;if mouse then 
&#9;&#9;mouse.KeyDown:connect(stunThemAll)
&#9;end
end

function onUnequipped()
&#9;if guitarSound then guitarSound:Stop() end
&#9;if guitarAnim then guitarAnim:Stop() end
&#9;if guitarClone then guitarClone:Remove() end
&#9;if stunGuitarClone then stunGuitarClone:Remove() end
&#9;Guitar.Transparency = 0.0
&#9;local sounds = Guitar:GetChildren()
&#9;for i = 1, #sounds do 
&#9;&#9;if sounds[i].ClassName == &quot;Sound&quot; then sounds[i]:Stop() end 
&#9;end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
</ProtectedString>
