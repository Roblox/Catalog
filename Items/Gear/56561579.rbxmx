				<ProtectedString name="Source">local Tool = script.Parent
local Book = Tool.Handle

local orb 
local spike 
local root

local sg

local vCharacter
local vPlayer

-- Animations
local bookAnim

function createGui()
	vCharacter = Tool.Parent
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)

	sg = Instance.new(&quot;ScreenGui&quot;)
	sg.Name = &quot;SpellGui&quot;
	if vPlayer then sg.Parent = vPlayer.PlayerGui end

	local frame = Instance.new(&quot;Frame&quot;)
	frame.Name = &quot;SpellGuiFrame&quot;
	frame.BackgroundTransparency = 1.0
	frame.Position = UDim2.new(0, 0, 0.35, 0)
	frame.Visible = true
	frame.Parent = sg
	frame.Size = UDim2.new(0, 70, 0, 210)

	local orbBtn = Instance.new(&quot;ImageButton&quot;)
	orbBtn.Name = &quot;Orb&quot;
	orbBtn.Image = &quot;http://www.roblox.com/asset/?id=56465749&quot;
	orbBtn.Size = UDim2.new(0, 64, 0, 64)
	orbBtn.Position = UDim2.new(0, 0, 0, 0)
	orbBtn.Parent = frame 
	orbBtn.MouseButton1Click:connect(function() 	
		root = Tool:FindFirstChild(&quot;Root&quot;) 
		orb = Tool:FindFirstChild(&quot;Orb&quot;)
		spike = Tool:FindFirstChild(&quot;Spike&quot;)
		if orb and orb.Value == 0 then 
			orb.Value = 1 
		end 
		if root and root.Value == 1 then root.Value = 0 end
		if spike and spike.Value == 1 then spike.Value = 0 end
		end)

	local spikeBtn = Instance.new(&quot;ImageButton&quot;)
	spikeBtn.Image = &quot;http://www.roblox.com/asset/?id=56465796&quot;
	spikeBtn.Size = UDim2.new(0, 64, 0, 64)
	spikeBtn.Position = UDim2.new(0, 0, 0, 70)
	spikeBtn.Parent = frame
	spikeBtn.Name = &quot;Spike&quot;
	spikeBtn.MouseButton1Click:connect(function() 	
		root = Tool:FindFirstChild(&quot;Root&quot;) 
		orb = Tool:FindFirstChild(&quot;Orb&quot;)
		spike = Tool:FindFirstChild(&quot;Spike&quot;)
		if spike and spike.Value == 0 then 
			spike.Value = 1 
		end 
		if orb and orb.Value == 1 then orb.Value = 0 end
		if root and root.Value == 1 then root.Value = 0 end
		end)

	local rootBtn = Instance.new(&quot;ImageButton&quot;)
	rootBtn.Image = &quot;http://www.roblox.com/asset/?id=56465831&quot;
	rootBtn.Size = UDim2.new(0, 64, 0, 64)
	rootBtn.Position = UDim2.new(0, 0, 0, 140)
	rootBtn.Parent = frame
	rootBtn.Name = &quot;Root&quot; 
	rootBtn.MouseButton1Click:connect(function() 
		root = Tool:FindFirstChild(&quot;Root&quot;) 
		orb = Tool:FindFirstChild(&quot;Orb&quot;)
		spike = Tool:FindFirstChild(&quot;Spike&quot;)
		if root and root.Value == 0 then 
			root.Value = 1 
		end 
		if orb and orb.Value == 1 then orb.Value = 0 end
		if spike and spike.Value == 1 then spike.Value = 0 end
		end)
end

function onActivated()
	if Tool.Enabled == false then return end
	vCharacter = Tool.Parent
	myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
	if myHumanoid == nil then 
		return
	end	
	bookAnim = myHumanoid:LoadAnimation(Tool.BookAnim)
	if bookAnim then bookAnim:Play()	end
end

function onEquipped(mouse)
	if mouse then 
		--mouse.KeyDown:connect(onKeyDown)
	end
	vCharacter = Tool.Parent
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	createGui() 
end

function onUnequipped()
	if sg then sg:Remove() end
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
Tool.Activated:connect(onActivated)
