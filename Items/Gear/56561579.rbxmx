				<ProtectedString name="Source">local Tool = script.Parent
local Book = Tool.Handle

Tool.Enabled = true
local vCharacter

local leftHandWeld
local bookClone
local spark

local bookSound 

-- Coroutines
local spikeCo
local orbCo
local rootCo

local orb
local root
local spike

local sgRef
local frameRef
local rootBtnRef
local orbBtnRef
local spikeBtnRef

local orbDamage = 25
local spikeDamage = 10

local spikeClone

local debris = game:GetService(&quot;Debris&quot;)

local spikePart = Instance.new(&quot;Part&quot;)
spikePart.Size = Vector3.new(20, 4.5, 22)
spikePart.Shape = &quot;Block&quot;
spikePart.FormFactor = 3
spikePart.CanCollide = false 
spikePart.CFrame = spikePart.CFrame 

local spikeMesh = Instance.new(&quot;SpecialMesh&quot;)
spikeMesh.MeshId = &quot;http://www.roblox.com/asset/?id=56440892&quot;
spikeMesh.TextureId = &quot;http://www.roblox.com/asset/?id=56441142&quot;
spikeMesh.Scale = Vector3.new(0.5, 0.5, 0.5)
spikeMesh.Parent = spikePart

orbPart = Instance.new(&quot;Part&quot;)
orbPart.Size = Vector3.new(0.5, 0.5, 0.5)
orbPart.Transparency = 0.8
orbPart.Shape = &quot;Ball&quot;
orbPart.FormFactor = 3
orbPart.Name =&quot;Orb&quot;
orbPart.CanCollide = false

local orbPartMesh = Instance.new(&quot;SpecialMesh&quot;)
orbPartMesh.MeshType = &quot;FileMesh&quot;
orbPartMesh.Name = &quot;Mesh&quot;
orbPartMesh.MeshId = &quot;http://www.roblox.com/asset/?id=48423624&quot;
orbPartMesh.TextureId = &quot;http://www.roblox.com/asset/?id=48598142&quot;
orbPartMesh.Scale = Vector3.new(0.04, 0.04, 0.04)
orbPartMesh.Parent = orbPart

local fire = Instance.new(&quot;Fire&quot;)
fire.Size = 0.1
fire.Heat = 5.0
fire.Color = Color3.new(139/255, 0, 139/255)
fire.Parent = orbPart 


local float = Instance.new(&quot;BodyForce&quot;)
float.force = Vector3.new(0.0, orbPart:GetMass() * 196.1, 0.0)
float.Name = &quot;Floating&quot; 

function orbCD()
	wait(5)
	orb = Tool:FindFirstChild(&quot;Orb&quot;)
	if orb then orb.Value = 0 end
	if orbBtnRef then orbBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56465749&quot; orbBtnRef.Active = true end
end

function spikeCD()
	wait(3)
	if spikeClone then spikeClone:Remove() end
	if myHumanoid then myHumanoid.WalkSpeed = 16.0 end
	wait(12)
	spike = Tool:FindFirstChild(&quot;Spike&quot;)
	if spike then spike.Value = 0 end 
	if spikeBtnRef then spikeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56465796&quot; spikeBtnRef.Active = true end
end

function rootCD()
	wait(15)
	root = Tool:FindFirstChild(&quot;Root&quot;)
	if root then root.Value = 0 end
	if rootBtnRef then rootBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56465831&quot; rootBtnRef.Active = true end 
end

function blow(parent, dmg,  hit)
	if parent then 
		if hit.Parent then 
			humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
		end
		vCharacter = Tool.Parent
		myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
		vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
		if humanoid ~= nil and myHumanoid ~= humanoid and myHumanoid ~= nil then 
			tagHumanoid(humanoid, vPlayer)
			humanoid:TakeDamage(dmg)
		end			
	end
end

function tagHumanoid(humanoid, player)
	if humanoid ~= nil then
		creator = Instance.new(&quot;ObjectValue&quot;)
		creator.Name = &quot;creator&quot;
		creator.Value = player
		creator.Parent = humanoid
		debris:AddItem(creator, 1)
	end
end

function shoot(pos)
     vCharacter = Tool.Parent
     vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
     if vCharacter == nil or vPlayer == nil then
          return
     end

     local spawnPos = vCharacter.Torso.Position
     spawnPos = spawnPos + (pos * 5.0)
     
     orbProjectile = orbPart:Clone()
     orbProjectile.Name = &quot;Orb&quot;
     orbProjectile.CFrame = CFrame.new(spawnPos, spawnPos + pos)

     floatForce = float:Clone()
     floatForce.Parent = orbProjectile
     orbProjectile.Velocity = pos * 65.0     
     
     orbProjectile.Parent = game.Workspace

     orbProjectile.Touched:connect(function(hit) blow(orbProjectile, orbDamage,  hit) end)

     debris:addItem(orbProjectile, 4)
end 

function onActivated()
	if Tool.Enabled == false then 
		return 	
	end

	Tool.Enabled = false 

	vCharacter = Tool.Parent
	myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
	myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
	if myTorso == nil or myHumanoid == nil then 
		return
	end

	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	if vPlayer == nil then 
		return
	end
	
	orb = Tool:FindFirstChild(&quot;Orb&quot;)
	root = Tool:FindFirstChild(&quot;Root&quot;)
	spike = Tool:FindFirstChild(&quot;Spike&quot;)

	sgRef = vPlayer.PlayerGui:FindFirstChild(&quot;SpellGui&quot;)
	frameRef = sgRef:FindFirstChild(&quot;SpellGuiFrame&quot;)

	if orb.Value == 1 or spike.Value == 1 or root.Value == 1 then
			
			bookSound = Tool:FindFirstChild(&quot;BookSound&quot;)
			if bookSound then bookSound:Play() end

			leftArm = vCharacter[&quot;Left Arm&quot;]

			if leftArm then
				bookClone = Book:Clone() 
				bookClone.Parent = vCharacter
				spark = Instance.new(&quot;Sparkles&quot;)	
				spark.Color = Color3.new(139/255, 0, 139/255)
				spark.Parent = bookClone 	
				bookWeld = Instance.new(&quot;Weld&quot;)
				bookWeld.Part0 = leftArm
				bookWeld.Part1 = bookClone
				bookWeld.Name = &quot;LeftGrip&quot;
				bookWeld.Parent = leftArm		
				bookWeld.C1 = CFrame.new(Vector3.new(-1.2, 0.0, 0.0)) * CFrame.Angles(math.pi/2, 0.0, math.pi/2)	
				Book.Transparency = 1.0
			end

		if orb.Value == 1 and (orbCo == nil or coroutine.status(orbCo) == &apos;dead&apos;) then 
			orbCo = coroutine.create(orbCD)
			coroutine.resume(orbCo)
			if frameRef then 
				orbBtnRef = frameRef:FindFirstChild(&quot;Orb&quot;) 
				if orbBtnRef then orbBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56508337&quot; orbBtnRef.Active = false end 
			end
			target = myHumanoid.TargetPoint
			lookAt = (target - myTorso.Position).unit 
			shoot(lookAt) 
		elseif root.Value == 1 and (rootCo == nil or coroutine.status(rootCo) == &apos;dead&apos;) then
			rootCo = coroutine.create(rootCD)
			coroutine.resume(rootCo)	
			if frameRef then 
				rootBtnRef = frameRef:FindFirstChild(&quot;Root&quot;) 
				if rootBtnRef then rootBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56508371&quot; rootBtnRef.Active = false end 
			end
			players = game.Players:GetChildren() 
			for i = 1, #players do 
				if players[i] ~= vPlayer then 
					tempChar = players[i].Character
					if tempChar then 
						tempHumanoid = tempChar:FindFirstChild(&quot;Humanoid&quot;) 
						tempTorso = tempChar:FindFirstChild(&quot;Torso&quot;)	
						if tempHumanoid and tempTorso then 
							distance = (tempTorso.Position - myTorso.Position).magnitude 
							if distance &lt; 30.0 then
								if tempChar:FindFirstChild(&quot;RootScript&quot;) == nil then 								
								newScript = Tool.RootScript:Clone()
								newScript.Parent = tempChar
								newScript.Disabled = false 
								end
							end
						end 
					end
				end
			end
		elseif spike.Value == 1 and (spikeCo == nil or coroutine.status(spikeCo) == &apos;dead&apos;) then
			spikeCo = coroutine.create(spikeCD)
			coroutine.resume(spikeCo)
			if frameRef then 
				spikeBtnRef = frameRef:FindFirstChild(&quot;Spike&quot;) 
				if spikeBtnRef then spikeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=56508353&quot; spikeBtnRef.Active = false end 
			end
			local SpeedScript = Tool:FindFirstChild(&quot;SpeedScript&quot;)
			if SpeedScript then
				SpeedScript = SpeedScript:Clone()
				SpeedScript.Parent = vCharacter
				SpeedScript.Disabled = false
			end
			spikeClone = spikePart:Clone()						
			spikeClone.CanCollide = false
			spikeClone.Anchored = true
			spikeClone.CFrame = myTorso.CFrame * CFrame.Angles(0, 0, math.pi)
			spikeClone.Parent = game.Workspace
			spikeClone.Touched:connect(function(hit) blow(spikeClone, spikeDamage, hit) end)
		end
		wait(4.0)
		spark:Remove()		
		bookClone:Remove()
		Book.Transparency = 0.0
	end
	Tool.Enabled = true
end

function onEquipped()
end

function onUnequipped()
	if bookClone then bookClone:Remove() end
	Book.Transparency = 0.0
	if spark then spark:Remove() end
	if bookSound then bookSound:Stop() end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
</ProtectedString>
