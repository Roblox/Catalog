local e = Instance.new(&quot;Explosion&quot;)
e.Name =&quot;EXPLODE&quot;
e.Parent = game.Workspace
e.Position = egg.Position
e.BlastPressure = 500000
e.BlastRadius = 7.5

debris:addItem(e, 2.0)

if egg then egg:Remove() end
