				<string name="Name">SwordScript</string>
				<ProtectedString name="Source">-- Allows the wielder to become a boss 
-- Destroy all those flyers with this
-- Who is the Kami now? 

-- 20 second cooldown
-- Add a RBXGear lib that allows for unified GUIs 
local function waitForChild(parent, instance)
&#9;while parent:FindFirstChild(instance) == nil do 
&#9;&#9;print(instance) 
&#9;&#9;wait() 
&#9;end 
&#9;return parent:FindFirstChild(instance)
end 

local Tool = script.Parent 
local Sword = Tool.Handle 

local vCharacter
local vPlayer
local vTorso
local vHumanoid 

Tool.Enabled = true 

-- The coolness factor 
local Wing_BoolValue = waitForChild(Tool, &quot;Wing&quot;)
local Wing_Weld = waitForChild(Tool, &quot;WingWeld&quot;)  
local Fire_Part = waitForChild(Sword, &quot;FirePart&quot;)
local Wing_Fire = waitForChild(Fire_Part, &quot;WingFire&quot;) 

-- Flat damage, it also has burst using the WingCutter
local Damage = 19

local RBX_Debris = game:GetService(&apos;Debris&apos;)
local RBX_Players = game:GetService(&apos;Players&apos;)

local TimeOfDay 

local Effect_Part = Instance.new(&apos;Part&apos;)
Effect_Part.FormFactor =3
Effect_Part.Size = Vector3.new(2, 2, 2)
Effect_Part.Name = &apos;WingCutter&apos;

-- TODO: Add the mesh and texture IDs 
local Effect_Mesh = Instance.new(&apos;SpecialMesh&apos;)
Effect_Mesh.Scale = Vector3.new(1, 1, 1)
Effect_Mesh.Parent = Effect_Part

local Effect_Fire = Instance.new(&apos;Fire&apos;)
Effect_Fire.Heat = 25 
Effect_Fire.Size = 5.0
Effect_Fire.Parent = Effect_Part

function blow(hit)&#9;
&#9;if hit and hit.Parent then 
&#9;&#9;local humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;if hit.Parent:IsA(&quot;Hat&quot;) then
&#9;&#9;&#9;humanoid = hit.Parent.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;end 
&#9;&#9;if humanoid and humanoid ~= vHumanoid and vHumanoid then &#9;&#9;&#9;
&#9;&#9;&#9;humanoid:TakeDamage(Damage)
&#9;&#9;&#9;if Wing_Fire and Wing_Fire.Enabled == true then
&#9;&#9;&#9;&#9;coroutine.resume(coroutine.create(function()
&#9;&#9;&#9;&#9;&#9;if humanoid and humanoid.Parent and humanoid.Parent:FindFirstChild(&quot;Torso&quot;) and not humanoid.Parent.Torso:FindFirstChild(&quot;DarkFire&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;local cFire = Wing_Fire:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;cFire.Parent = humanoid.Parent:FindFirstChild(&quot;Torso&quot;) 
&#9;&#9;&#9;&#9;&#9;&#9;RBX_Debris:AddItem(cFire, 4) 
&#9;&#9;&#9;&#9;&#9;&#9;local duration = 4
&#9;&#9;&#9;&#9;&#9;&#9;while duration &gt; 0 do
&#9;&#9;&#9;&#9;&#9;&#9;&#9;humanoid:TakeDamage(5)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;duration = duration - 1 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;wait(1.0) 
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;end)) 
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;end 
end

function onEquipped() &#9;
&#9;if Wing_Weld then 
&#9;&#9;Wing_Weld.Parent = Tool 
&#9;end 
&#9;if Wing_Fire then 
&#9;&#9;Wing_Fire.Enabled = true
&#9;end 
&#9;vCharacter = Tool.Parent 
&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;vHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;vTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
end 

function onUnequipped()&#9;
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
Sword.Touched:connect(blow)

Wing_BoolValue.Changed:connect(function()
&#9;if Wing_BoolValue and Wing_BoolValue.Value == true and vTorso then
&#9;&#9;local players = RBX_Players:GetPlayers() 
&#9;&#9;for i = 1, #players do 
&#9;&#9;&#9;if players[i] and players[i] ~= vPlayer and players[i].Character then 
&#9;&#9;&#9;&#9;local torso = players[i].Character:FindFirstChild(&apos;Torso&apos;)
&#9;&#9;&#9;&#9;if torso then
&#9;&#9;&#9;&#9;&#9;local humanoid = players[i].Character:FindFirstChild(&apos;Humanoid&apos;)
&#9;&#9;&#9;&#9;&#9;local distance = (vTorso.Position - torso.Position).magnitude 
&#9;&#9;&#9;&#9;&#9;local tChildren = torso:GetChildren() 
&#9;&#9;&#9;&#9;&#9;local tabooCount = 0 
&#9;&#9;&#9;&#9;&#9;-- All flight scripts will have these 3 forces
&#9;&#9;&#9;&#9;&#9;-- Need to unify this, but for now this will nuke all of them 
&#9;&#9;&#9;&#9;&#9;local taboo_List = {[&apos;FlightSpin&apos;] = true, [&apos;FlightPower&apos;] = true, [&apos;FlightHold&apos;] = true}
&#9;&#9;&#9;&#9;&#9;for j = 1, #tChildren do 
&#9;&#9;&#9;&#9;&#9;&#9;if taboo_List[tChildren[j].Name] then 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;tabooCount = tabooCount + 1
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;if distance &lt; 120 and tabooCount &gt;= 3 then 
&#9;&#9;&#9;&#9;&#9;&#9;local ePart = Effect_Part:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;ePart.CFrame = vTorso.CFrame 
&#9;&#9;&#9;&#9;&#9;&#9;ePart.CanCollide = false 

&#9;&#9;&#9;&#9;&#9;&#9;local bForce = Instance.new(&apos;BodyForce&apos;)
&#9;&#9;&#9;&#9;&#9;&#9;bForce.Name = &apos;Float&apos;
&#9;&#9;&#9;&#9;&#9;&#9;bForce.force = Vector3.new(0, ePart:GetMass() * 196.2, 0)
&#9;&#9;&#9;&#9;&#9;&#9;bForce.Parent = ePart 

&#9;&#9;&#9;&#9;&#9;&#9;-- I know, this is super fast, but that&apos;s how I roll 
&#9;&#9;&#9;&#9;&#9;&#9;ePart.Velocity = (torso.Position - vTorso.Position).unit * 200.0 
&#9;&#9;&#9;&#9;&#9;&#9;ePart.Parent = game.Workspace

&#9;&#9;&#9;&#9;&#9;&#9;RBX_Debris:AddItem(ePart, 5.0)

&#9;&#9;&#9;&#9;&#9;&#9;ePart.Touched:connect(function(hit) 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;if hit and hit.Parent and torso and hit.Parent == torso then 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;if humanoid then 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;humanoid:TakeDamage(65) 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;&#9;end) 

&#9;&#9;&#9;&#9;&#9;&#9;-- Now, to add the actual effect, lets make this cool 
&#9;&#9;&#9;&#9;&#9;&#9;if torso.Parent:FindFirstChild(&apos;UnwingScript&apos;) == nil then 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;local tScript = script.UnwingScript:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;&#9;tScript.Parent = torso.Parent 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;tScript.Disabled = false 
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end 
&#9;&#9;end 

&#9;&#9;wait(20.0)
&#9;&#9;Wing_BoolValue.Value = false 
&#9;end 
end)





</ProtectedString>
			</Properties>
			<Item class="Script" referent="RBX12">
				<Properties>
