				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">RezScript</string>
				<ProtectedString name="Source">local Handle= script.Parent.Handle
local Resolution=50
local selectPart=Instance.new(&apos;SelectionBox&apos;)
selectPart.Visible=true
selectPart.Color=BrickColor.new(&apos;Medium blue&apos;)

function updatePosition(part,follow,offset,timelength)
&#9;local initTime=time()
&#9;while time()-initTime&lt;timelength and part do
&#9;&#9;part.CFrame=CFrame.new(follow.CFrame*offset)--,follow.CFrame.lookVector+follow.CFrame.p)
&#9;&#9;wait()
&#9;end
end

function lerpTransparency(part, itrans, ftrans, length)
&#9;local initTime=time()
&#9;while time()-initTime&lt;length do
&#9;&#9;part.Transparency=itrans+((ftrans-itrans)*((time()-initTime)/length))
&#9;&#9;wait()
&#9;end
&#9;part.Transparency=ftrans
end

local inrez= false

function rezIn()
&#9;if inrez then return end
&#9;inrez=true
&#9;Handle.EquipSound:Play()
&#9;Handle.Transparency=1
&#9;local rezParts={}
&#9;local selectBoxes = {}
&#9;local Hsize=Handle.Size
&#9;wait()
&#9;for i=1,Resolution, 1 do
&#9;&#9;rezParts[i]=Instance.new(&apos;Part&apos;)
&#9;&#9;rezParts[i].Anchored=true
&#9;&#9;rezParts[i].Parent=game.Lighting
&#9;&#9;rezParts[i].Size=Vector3.new(Hsize.x/2,(Hsize.y*2)/Resolution,Hsize.z/2)
&#9;&#9;--Spawn(function() updatePosition(rezParts[i],Handle,Vector3.new(0,0,Resolution)*i, 5) end)
&#9;&#9;Spawn(function() updatePosition(rezParts[i],Handle,Vector3.new(0,(((Hsize.y*2)/Resolution)*i)-4.5,0), 10) end)
&#9;&#9;selectBoxes[i]=selectPart:Clone()
&#9;&#9;selectBoxes[i].Color=BrickColor.new(&apos;Bright blue&apos;)
&#9;&#9;selectBoxes[i].Parent=script.Parent
&#9;&#9;selectBoxes[i].Transparency=0
&#9;&#9;selectBoxes[i].Adornee=rezParts[i]
&#9;&#9;wait(.03)
&#9;&#9;Spawn(function() lerpTransparency(selectBoxes[i],1,.1, 4); lerpTransparency(selectBoxes[i],.1, 1, .5) end)
&#9;&#9;selectBoxes[i].Color=BrickColor.new(&apos;Medium blue&apos;)
&#9;end

&#9;wait(3)
&#9;Handle.Transparency=0
&#9;wait(2)
&#9;for i=1,Resolution, 1 do
&#9;&#9;rezParts[i]:Destroy()
&#9;&#9;selectBoxes[i]:Destroy()
