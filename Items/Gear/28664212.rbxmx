				<string name="ScriptGuid">{2FC375F1-4347-4A9A-946E-4B6C3913B8B5}</string>
				<ProtectedString name="Source"><![CDATA[Tool = script.Parent;
local arms = nil
local torso = nil
local welds = {}

local function getArms(torso)
    return {
		torso.Parent:FindFirstChild("Left Arm") or torso.Parent:FindFirstChild("LeftUpperArm"),
		torso.Parent:FindFirstChild("Right Arm") or torso.Parent:FindFirstChild("RightUpperArm")
	}
end

local function getSH(torso)
	local arms = getArms(torso)

    return {
		torso:FindFirstChild("Left Shoulder") or arms[1]:FindFirstChild("LeftShoulder"),
		torso:FindFirstChild("Right Shoulder") or arms[2]:FindFirstChild("RightShoulder")
	}
end

function Equip(mouse)
    wait(0.01)
    torso = Tool.Parent:FindFirstChild("Torso") or Tool.Parent:FindFirstChild("UpperTorso")
    arms = getArms(torso)

    if arms ~= nil and torso ~= nil then
		local sh = getSH(torso)
        if sh ~= nil then
            local yes = true
            if yes then
                yes = false
                sh[1].Part1 = nil
                sh[2].Part1 = nil
                local weld1 = Instance.new("Weld")
                weld1.Part0 = torso
                weld1.Parent = torso
                weld1.Part1 = arms[1]
                weld1.C1 = CFrame.new(1.5,0,0) * CFrame.fromEulerAnglesXYZ(0,0,0)
                welds[1] = weld1
                local weld2 = Instance.new("Weld")
                weld2.Part0 = torso
                weld2.Parent = torso
                weld2.Part1 = arms[2]
                weld2.C1 = CFrame.new(-1.5,0,0) * CFrame.fromEulerAnglesXYZ(0,0,0)
                welds[2] = weld2
            end
        end
    end
end

function Unequip(mouse)
    if arms ~= nil and torso ~= nil then
		local sh = getSH(torso)
        if sh ~= nil then
            local yes = true
            if yes then
                yes = false
                sh[1].Part1 = arms[1]
                sh[2].Part1 = arms[2]
                welds[1].Parent = nil
                welds[2].Parent = nil
            end
        end
    end
end
Tool.Equipped:connect(Equip)
Tool.Unequipped:connect(Unequip)
]]></ProtectedString>
