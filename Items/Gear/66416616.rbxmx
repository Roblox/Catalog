				<ProtectedString name="Source"><![CDATA[local Tool = script.Parent
local Bolt = Tool.Handle
local Event = Tool.ThunderScript.BoltEvent

local uis, cas = game:GetService'UserInputService', game:GetService'ContextActionService'

local vCharacter
local myHumanoid 
local myTorso 

local activateWrathOfGod 
local e

function unleashWrath(key)
	if key:lower() == "q" then 
		activateWrathOfGod = Tool:FindFirstChild("WrathOfGod")
		if activateWrathOfGod and activateWrathOfGod.Value == false then 
			Event:FireServer("qDown")
		end 
	end 
	if key:lower() == "e" then 
		e = Tool:FindFirstChild("E")
		if e and e.Value == false then 
			Event:FireServer("eDown")
		end 
	end 
end 

--[[function keyUp(key)
	if key:lower() == "e" then
		e = Tool:FindFirstChild("E")
		if e and e.Value == true then 
			Event:FireServer("eUp")
		end 
	end
end]]

function onEquipped(mouse)
	vCharacter = Tool.Parent 
	myHumanoid = vCharacter:FindFirstChild("Humanoid")
	myTorso = vCharacter:FindFirstChild("Torso")
	if mouse then 
		mouse.KeyDown:connect(unleashWrath)
		--mouse.KeyUp:connect(keyUp)
	end 
end

if uis.TouchEnabled then
	Tool.Equipped:connect(function()
		local setup = true
		cas:UnbindAction('Thunderbolt_Q')
		cas:UnbindAction('Thunderbolt_E')
		
		cas:BindActionToInputTypes('Thunderbolt_Q', function() 
			if setup == false then
				Event:FireServer("qDown")
			end
		end, true, '')
		cas:BindActionToInputTypes('Thunderbolt_E', function()
			if setup == false then
				Event:FireServer("eDown")
			end
		end, true, '')
		
		cas:SetTitle('Thunderbolt_Q', 'Thunder')
		cas:SetTitle('Thunderbolt_E', 'Shield')
		setup = false
	end)
	
	Tool.Unequipped:connect(function()
		cas:UnbindAction('Thunderbolt_Q')
		cas:UnbindAction('Thunderbolt_E')
	end)
end

Tool.Equipped:connect(onEquipped)

]]></ProtectedString>
