&#9;&#9;&#9;Part0 = rightarm,
&#9;&#9;&#9;Part1 = handle,
&#9;&#9;}
&#9;&#9;--kill the old weld
&#9;&#9;rightGrip.Part1 = nil
&#9;&#9;rightGrip.Part0 = nil
&#9;&#9;Motor6DGrip.Parent = rightGrip.Parent
&#9;&#9;Motor6DGrip.C1 = Motor6DGrip.C1 * CFrame.Angles(0, -math.pi/2, 0)
&#9;&#9;Motor6DGrip.C1 = Motor6DGrip.C1 * CFrame.Angles(math.pi/2, 0, 0)
&#9;&#9;Motor6DGrip.C1 = Motor6DGrip.C1 + Vector3.new(0.20, 0, 0)
&#9;end
end)

