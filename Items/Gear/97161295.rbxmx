				<string name="Name">LocalScript</string>
				<ProtectedString name="Source">--Made by Stickmasterluke


sp=script.Parent
plr=game.Players.LocalPlayer

rate=1/30
debris=game:GetService(&quot;Debris&quot;)
canclone=true

function waitfor(parent,name)
&#9;while parent:FindFirstChild(name)==nil do
&#9;&#9;wait()
&#9;end
&#9;return parent:FindFirstChild(name)
end

moveint=22
scrollspeed=.1

function creategui()
&#9;tool=nil
&#9;if plr:FindFirstChild(&quot;PlayerGui&quot;)~=nil and plr:FindFirstChild(&quot;Backpack&quot;)~=nil then
&#9;&#9;if gui~=nil then
&#9;&#9;&#9;gui:remove()
&#9;&#9;end
&#9;&#9;gui=Instance.new(&quot;ScreenGui&quot;)
&#9;&#9;gui.Name=&quot;ToolCloneGui&quot;
&#9;&#9;gui.Parent=plr.PlayerGui

&#9;&#9;showframe=Instance.new(&quot;Frame&quot;)
&#9;&#9;showframe.Style=&quot;RobloxRound&quot;
&#9;&#9;showframe.Position = UDim2.new(1,-480,1,-410)
&#9;&#9;showframe.Size = UDim2.new(0,160,0,200)
&#9;&#9;showframe.ClipsDescendants=true
&#9;&#9;showframe.Visible=false
&#9;&#9;showframe.Parent = gui

&#9;&#9;showimage=Instance.new(&quot;ImageLabel&quot;)
&#9;&#9;showimage.Size=UDim2.new(1,0,0,150)
&#9;&#9;showimage.BackgroundTransparency=1
&#9;&#9;showimage.Parent=showframe

&#9;&#9;closebutton=Instance.new(&quot;TextButton&quot;)
&#9;&#9;closebutton.Text=&quot;X&quot;
&#9;&#9;closebutton.TextScaled=true
&#9;&#9;closebutton.Style=&quot;RobloxButtonDefault&quot;
&#9;&#9;closebutton.Size=UDim2.new(0,30,0,30)
&#9;&#9;closebutton.TextColor3=Color3.new(1,0,0)
&#9;&#9;closebutton.Font=&quot;Arial&quot;
&#9;&#9;closebutton.FontSize=&quot;Size14&quot;
&#9;&#9;closebutton.Position=UDim2.new(1,-20,0,-10)
&#9;&#9;closebutton.Parent=showframe
&#9;&#9;closebutton.MouseButton1Down:connect(function()
&#9;&#9;&#9;showframe.Visible=false
&#9;&#9;end)

&#9;&#9;clonebutton=Instance.new(&quot;TextButton&quot;)
&#9;&#9;clonebutton.Text=&quot;Clone&quot;
&#9;&#9;clonebutton.Style=&quot;RobloxButton&quot;
&#9;&#9;clonebutton.Size=UDim2.new(0,80,0,30)
&#9;&#9;clonebutton.TextColor3=Color3.new(1,1,1)
&#9;&#9;clonebutton.Font=&quot;ArialBold&quot;
&#9;&#9;clonebutton.FontSize=&quot;Size14&quot;
&#9;&#9;clonebutton.Position=UDim2.new(.5,-40,1,-35)
&#9;&#9;clonebutton.Parent=showframe
&#9;&#9;clonebutton.MouseButton1Down:connect(function()
&#9;&#9;&#9;local chr=plr.Character
&#9;&#9;&#9;if tool~=nil and chr~=nil then
&#9;&#9;&#9;&#9;local ctool=tool:clone()
&#9;&#9;&#9;&#9;--ctool.CanBeDropped=true
&#9;&#9;&#9;&#9;local t=chr:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;&#9;local hndl=ctool:FindFirstChild(&quot;Handle&quot;)
&#9;&#9;&#9;&#9;if hndl~=nil and t~=nil and canclone then
&#9;&#9;&#9;&#9;&#9;canclone=false
&#9;&#9;&#9;&#9;&#9;gui:remove()
&#9;&#9;&#9;&#9;&#9;hndl.CFrame=t.CFrame+(t.CFrame.lookVector*10)+Vector3.new(0,5,0)
&#9;&#9;&#9;&#9;&#9;ctool.Parent=game.Workspace
&#9;&#9;&#9;&#9;&#9;local s=Instance.new(&quot;Sound&quot;)
&#9;&#9;&#9;&#9;&#9;s.Name=&quot;PhaseInSoundEffect&quot;
&#9;&#9;&#9;&#9;&#9;s.SoundId=&quot;http://www.roblox.com/asset/?id=97272128&quot;
&#9;&#9;&#9;&#9;&#9;debris:AddItem(s,5)
&#9;&#9;&#9;&#9;&#9;s.Parent=hndl
&#9;&#9;&#9;&#9;&#9;delay(.05,function()
&#9;&#9;&#9;&#9;&#9;&#9;if s~=nil then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;s:Play()
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end)
&#9;&#9;&#9;&#9;&#9;delay(0,function()
&#9;&#9;&#9;&#9;&#9;&#9;p=Instance.new(&quot;Part&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;p.BrickColor=BrickColor.new(&quot;Cyan&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;p.FormFactor=&quot;Custom&quot;
&#9;&#9;&#9;&#9;&#9;&#9;p.Size=Vector3.new(0,0,0)
&#9;&#9;&#9;&#9;&#9;&#9;p.Anchored=true
&#9;&#9;&#9;&#9;&#9;&#9;p.CanCollide=false
&#9;&#9;&#9;&#9;&#9;&#9;p.TopSurface=&quot;Smooth&quot;
&#9;&#9;&#9;&#9;&#9;&#9;p.BottomSurface=&quot;Smooth&quot;
&#9;&#9;&#9;&#9;&#9;&#9;p.Name=&quot;Effect&quot;
&#9;&#9;&#9;&#9;&#9;&#9;local m=Instance.new(&quot;SpecialMesh&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;m.MeshType=&quot;Sphere&quot;
&#9;&#9;&#9;&#9;&#9;&#9;m.Parent=p
&#9;&#9;&#9;&#9;&#9;&#9;debris:AddItem(p,5)
&#9;&#9;&#9;&#9;&#9;&#9;p.CFrame=hndl.CFrame+Vector3.new(0,-3,0)
&#9;&#9;&#9;&#9;&#9;&#9;p.Parent=game.Workspace
&#9;&#9;&#9;&#9;&#9;&#9;frames=.5/rate
&#9;&#9;&#9;&#9;&#9;&#9;for frame=1,frames do
&#9;&#9;&#9;&#9;&#9;&#9;&#9;local percent=frame/frames
&#9;&#9;&#9;&#9;&#9;&#9;&#9;m.Scale=Vector3.new(5,5,5)*(1-percent)*8
&#9;&#9;&#9;&#9;&#9;&#9;&#9;p.Transparency=percent
&#9;&#9;&#9;&#9;&#9;&#9;&#9;wait(rate)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;sp:remove()
&#9;&#9;&#9;&#9;&#9;end)
&#9;&#9;&#9;&#9;&#9;wait(2)
&#9;&#9;&#9;&#9;&#9;sp:remove()
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end)

&#9;&#9;frame=Instance.new(&quot;Frame&quot;)
&#9;&#9;frame.Style=&quot;RobloxRound&quot;
&#9;&#9;frame.Position = UDim2.new(1,-300,1,-410)
&#9;&#9;frame.Size = UDim2.new(0,200,0,323)
&#9;&#9;frame.ClipsDescendants=true
&#9;&#9;frame.Parent = gui

&#9;&#9;window=Instance.new(&quot;Frame&quot;)
&#9;&#9;window.BackgroundTransparency=1
&#9;&#9;window.ClipsDescendants=true
&#9;&#9;window.Parent=frame

&#9;&#9;scroller=Instance.new(&quot;Frame&quot;)
&#9;&#9;scroller.BackgroundTransparency=.5
&#9;&#9;scroller.BorderSizePixel=0
&#9;&#9;scroller.Position = UDim2.new(1,-25,0,0)
&#9;&#9;scroller.Size = UDim2.new(0,25,1,0)
&#9;&#9;scroller.Parent = frame

&#9;&#9;local buttonup=Instance.new(&quot;ImageButton&quot;)
&#9;&#9;buttonup.Size=UDim2.new(0,25,0,25)
&#9;&#9;buttonup.Position=UDim2.new(0,0,0,0)
&#9;&#9;buttonup.Image=&quot;rbxasset://textures/ui/scrollbuttonUp.png&quot;
&#9;&#9;buttonup.BackgroundTransparency=1
&#9;&#9;buttonup.MouseButton1Down:connect(function()
&#9;&#9;&#9;upbuttondown=true
&#9;&#9;&#9;while upbuttondown do
&#9;&#9;&#9;&#9;if window.Position.Y.Offset+moveint&gt;=0 then
&#9;&#9;&#9;&#9;&#9;window.Position=UDim2.new(0,0,0,0)
&#9;&#9;&#9;&#9;&#9;upbuttondown=false
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;window.Position=window.Position+UDim2.new(0,0,0,moveint)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;wait(scrollspeed)
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;buttonup.MouseButton1Up:connect(function()
&#9;&#9;&#9;upbuttondown=false
&#9;&#9;end)
&#9;&#9;buttonup.MouseLeave:connect(function()
&#9;&#9;&#9;upbuttondown=false
&#9;&#9;end)
&#9;&#9;buttonup.Parent=scroller

&#9;&#9;local buttondown=Instance.new(&quot;ImageButton&quot;)
&#9;&#9;buttondown.Size=UDim2.new(0,25,0,25)
&#9;&#9;buttondown.Position=UDim2.new(0,0,1,-25)
&#9;&#9;buttondown.BackgroundTransparency=1
&#9;&#9;buttondown.Image=&quot;rbxasset://textures/ui/scrollbuttonDown.png&quot;
&#9;&#9;buttondown.MouseButton1Down:connect(function()
&#9;&#9;&#9;downbuttondown=true
&#9;&#9;&#9;while downbuttondown do
&#9;&#9;&#9;&#9;if (window.Position.Y.Offset+window.Size.Y.Offset)-moveint&lt;=frame.Size.Y.Offset then
&#9;&#9;&#9;&#9;&#9;window.Position=UDim2.new(0,0,0,frame.Size.Y.Offset-window.Size.Y.Offset)
&#9;&#9;&#9;&#9;&#9;downbuttondown=false
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;window.Position=window.Position+UDim2.new(0,0,0,-moveint)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;wait(scrollspeed)
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;buttondown.MouseButton1Up:connect(function()
&#9;&#9;&#9;downbuttondown=false
&#9;&#9;end)
&#9;&#9;buttondown.MouseLeave:connect(function()
&#9;&#9;&#9;downbuttondown=false
&#9;&#9;end)
&#9;&#9;buttondown.Parent=scroller

&#9;&#9;i=0
&#9;&#9;for _,v in ipairs(plr.Backpack:GetChildren()) do
&#9;&#9;&#9;if v.className==&quot;Tool&quot; then
&#9;&#9;&#9;&#9;local button=Instance.new(&quot;TextButton&quot;)
&#9;&#9;&#9;&#9;button.Size=UDim2.new(1,0,0,20)
&#9;&#9;&#9;&#9;button.Position=UDim2.new(0,0,0,i*22)
&#9;&#9;&#9;&#9;button.TextColor3=Color3.new(1,1,1)
&#9;&#9;&#9;&#9;button.Style=&quot;RobloxButton&quot;
&#9;&#9;&#9;&#9;button.Text=v.Name
&#9;&#9;&#9;&#9;button.Visible=true
&#9;&#9;&#9;&#9;button.Parent=window
&#9;&#9;&#9;&#9;button.MouseButton1Down:connect(function()
&#9;&#9;&#9;&#9;&#9;if v~=nil then
&#9;&#9;&#9;&#9;&#9;&#9;tool=v
&#9;&#9;&#9;&#9;&#9;&#9;showimage.Image=v.TextureId
&#9;&#9;&#9;&#9;&#9;&#9;showframe.Visible=true
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end)
&#9;&#9;&#9;&#9;i=i+1
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;window.Size=UDim2.new(1,-30,0,(i+1)*22)
&#9;end
end



function onEquipped(mouse)
&#9;equipped=true
&#9;plr=game.Players.LocalPlayer
&#9;if mouse~=nil and plr~=nil then
&#9;&#9;creategui()
&#9;end
end

function onUnequipped()
&#9;equipped=false
&#9;if gui~=nil then
&#9;&#9;gui:remove()
&#9;end
end

sp.Equipped:connect(onEquipped)
sp.Unequipped:connect(onUnequipped)


</ProtectedString>
