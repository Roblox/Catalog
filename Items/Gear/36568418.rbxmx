				<ProtectedString name="Source">local Tool = script.Parent
local humanoid = nil

function onEquipped()

&#9;humanoid = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;)

end

function onUnequipped()

&#9;if humanoid ~= nil then
&#9;&#9;if Tool.DanceObject.Value ~= nil then
