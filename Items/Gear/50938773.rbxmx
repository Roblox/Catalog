	humanoid.WalkSpeed = 2.0

	children = vChar:GetChildren()
	for i = 1, #children do 
		if children[i].className == &quot;Tool&quot; then 
			print(&quot;FOUND TOOL&quot;)
			children[i].Parent = game.Workspace
		end
	end	
end

wait(2.0)

humanoid.WalkSpeed = 16.0
script:Remove() 
