		<Item class="Script" referent="RBX5">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">BasketScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Basket = Tool.Handle

local vCharacter

Tool.Enabled = true

insert = game:GetService(&quot;InsertService&quot;)

local assets = {16726030, 29939404, 10472779, 22787248, 16214845, 15932306, 15177716}

function onActivated()
	if Tool.Enabled == false then
		return
	end

	Tool.Enabled = false
	vCharacter = Tool.Parent
	myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
	if myTorso == nil then 
		return
	end

	basketClone = Basket:Clone()
	basketClone.Parent = game.Workspace
	basketClone.CanCollide = true
	Basket.Transparency = 1.0
	
	wait(0.4)

	a = insert:LoadAsset(assets[math.random(1, 4)]) a = a:GetChildren() a=a[1] a.Parent = game.Workspace a.Handle.Position = myTorso.Position + (myTorso.CFrame.lookVector * 2.5)
	b = insert:LoadAsset(assets[math.random(3, 7)]) b = b:GetChildren() b=b[1] b.Parent = game.Workspace b.Handle.Position = myTorso.Position + (myTorso.CFrame.lookVector * 3.0)
	c = insert:LoadAsset(assets[math.random(2, 5)]) c = c:GetChildren() c=c[1] c.Parent = game.Workspace c.Handle.Position = myTorso.Position + (myTorso.CFrame.lookVector * 4.5)
	basketClone:Remove()
	Tool:Remove()
	wait(2.0)
	Tool.Enabled = true 
end

function onEquipped()
end

function onUnequipped()
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)</ProtectedString>
