local myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)

if myTorso == nil or myHumanoid == nil then 
&#9;script:Remove()
end

local duration = 7.0

while duration &gt; 0 do 
&#9;myHumanoid:TakeDamage(4.0)
&#9;duration = duration - wait(1.0)
&#9;wait(1.0)
end

wait(1.0)

script:Remove()
