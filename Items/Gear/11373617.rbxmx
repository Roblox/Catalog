				<string name="Name">WoodlandsStaff</string>
				<string name="Source">local Tool = script.Parent;

enabled = true



p = {}


Tool = script.Parent

local WoodWalk = Instance.new(&quot;Sound&quot;)
WoodWalk.SoundId = &quot;http://www.roblox.com/asset/?id=11387922&quot;
WoodWalk.Volume = .7
WoodWalk.Parent = Tool.Handle

function setTrans(root, trans, character)
	local c = root:GetChildren()
	for i=1,#c do
		setTrans(c[i], trans)
		if (c[i].className == &quot;Part&quot; and c[i]:IsDescendantOf(character)) then
			p[c[i]] = c[i].Transparency
			c[i].Transparency = trans
		end
	end
end

function resetTrans()
	WoodWalk:Stop()
	for k,v in pairs(p) do 
		k.Transparency = v
	end
end

function SetTransparency(root, trans)
	p = {}
	setTrans(root, trans)
	WoodWalk:Play()
end



function staffUp()
	Tool.GripForward = Vector3.new(0,0,-1)
	Tool.GripRight = Vector3.new(1,0,0)
	Tool.GripUp = Vector3.new(0,1,0)
end

function staffOut()
	Tool.GripForward = Vector3.new(.0976, .00976, -.995)
	Tool.GripRight = Vector3.new(.195, -.981, .00952)
	Tool.GripUp = Vector3.new(.976, .195, .0976)
end

function isTurbo(character)
	return character:FindFirstChild(&quot;TreeHelm&quot;) ~= nil
end


Tool.Enabled = true
function onActivated()
	if Tool.Enabled == false  then
		return
	end

	Tool.Enabled = false


	local character = Tool.Parent;
	local humanoid = character.Humanoid
	if humanoid == nil then
		print(&quot;Humanoid not found&quot;)
		return 
	end


	local trans = .94
	if (isTurbo(character) == true) then trans = .97 end

	staffOut()
	wait(.5)
	staffUp()
	SetTransparency(Tool.Parent, trans, character)
	wait(8)
	resetTrans()
	wait(3)

	Tool.Enabled = true

end

function onEquipped()
	Tool.Handle.EquipSound:play()
end

function onUnequipped()

	resetTrans()
	

end

script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
script.Parent.Unequipped:connect(onUnequipped)
</string>
