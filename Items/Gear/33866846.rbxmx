				<ProtectedString name="Source">local Tool = script.Parent
local hit = nil
local out = nil
local humanoid = nil
local torso = nil
local enabled = false
local connection = nil
local reload = 5
local hammerStrength = 30
local upStrength = 1500
local children = {}
local toAffect = {}

local debris = game:GetService(&quot;Debris&quot;)

function onActivated()

	if enabled then
		return
	end
	
	enabled = true

	if hit ~= nil then
				
		makeHammerActive()
		hit:Play(0.1,1,1)
		connection = hit.KeyframeReached:connect(sendFlying)

		wait(reload)
	end

	enabled = false

end

function makeHammerActive()

	local part = Instance.new(&quot;Part&quot;)
	part.Transparency  =  1
	part.formFactor = 2
	part.Size = Vector3.new(1,0.4,1)
	
	local fire = Instance.new(&quot;Fire&quot;)
	fire.Color = Color3.new(155/255,1,206/255)
	fire.SecondaryColor = Color3.new(0,1,1)
	fire.Heat = -5
	fire.Size = 8
	fire.Parent = part

	part.Parent = game.Workspace

	local weld = Instance.new(&quot;Weld&quot;)
	weld.Part0 = part
	weld.Parent = part
	weld.Part1 = Tool.Handle
	weld.C0 = CFrame.new(0,-4.5,0)

	debris:AddItem(part,1.5)

end

function sendFlying(stringframe)

	if stringframe == &quot;sendflying&quot; then

		connection:disconnect()

		Tool.Handle.InitialHit:Play()
		Tool.Handle.Hit:Play()

		children = {}
		recursiveGetChildren(game.Workspace)

		for i = 1, #children do
			if children[i].Anchored == false then

				local dist = Vector3.new(children[i].Position.x - torso.Position.x,children[i].Position.y - torso.Position.y,children[i].Position.z - torso.Position.z)
				if dist.magnitude &lt;= hammerStrength then

					local bodyPos = Instance.new(&quot;BodyPosition&quot;)
					local height = hammerStrength * (1 - (dist.magnitude/hammerStrength))
					bodyPos.position = children[i].Position + Vector3.new(0,height,0)
					bodyPos.P = children[i]:GetMass() * upStrength
					bodyPos.D = bodyPos.P/2
					bodyPos.maxForce = Vector3.new(bodyPos.P,bodyPos.P,bodyPos.P)
					bodyPos.Parent = children[i]
					debris:AddItem(bodyPos,3)

					if children[i].Parent:FindFirstChild(&quot;Humanoid&quot;) == nil and children[i].Parent.Parent:FindFirstChild(&quot;Humanoid&quot;) == nil then
						children[i]:BreakJoints()
					end

					table.insert(toAffect,children[i])

				end

			end
		end


	end

	wait(3.1)
	sendOut()
	toAffect = {}

end

function sendForceFieldOut()

	local fire = Instance.new(&quot;Fire&quot;)
	fire.Color = Color3.new(155/255,1,206/255)
	fire.SecondaryColor = Color3.new(0,1,1)
	fire.Heat = -5
	fire.Size = 1
	fire.Parent = torso
	for i = 1, 8 do
		fire.Size = fire.Size + 4
		wait(0.2)
	end
	fire:remove()
end

function sendOut()

	
	if #toAffect &gt; 0 then

		Tool.Handle.SendOut:Play()

		local co = coroutine.create(sendForceFieldOut)
		coroutine.resume(co)

		out:Play()
		out.KeyframeReached:connect(function(string)
			if string == &quot;end&quot; then
				wait(0.4)
				out:Stop()
			end
		end)

	end

	wait(0.3)

	for i =1, #toAffect do

		if toAffect[i].Parent ~= nil and toAffect[i].Parent ~= torso.Parent then

			local dist = Vector3.new(torso.Position.x - toAffect[i].Position.x,torso.Position.y - toAffect[i].Position.y,torso.Position.z - toAffect[i].Position.z)

			local bodyForce = Instance.new(&quot;BodyForce&quot;)
			bodyForce.force = dist.unit * toAffect[i]:GetMass() * -400
			bodyForce.Parent = toAffect[i]

			if toAffect[i].Parent:FindFirstChild(&quot;Humanoid&quot;) ~= nil and toAffect[i].Name == &quot;Torso&quot; then
				toAffect[i].Parent:FindFirstChild(&quot;Humanoid&quot;).Sit = true
			end

			debris:AddItem(bodyForce,1)

		end

	end

end

function recursiveGetChildren(parent)

	local kids = parent:GetChildren()

	if kids ~= nil then
		for i =1, #kids do
			
			if kids[i].className == &quot;Part&quot; then
				table.insert(children,kids[i])
			end
			recursiveGetChildren(kids[i])

		end
	end

end

function onEquipped()

	humanoid = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;)
	torso = Tool.Parent:FindFirstChild(&quot;Torso&quot;)
	hit = humanoid:LoadAnimation(Tool.gravityslam)
	out = humanoid:LoadAnimation(Tool.staffout)
	
end

function onUnequipped()

	hit:Stop()
	hit:remove()
	out:Stop()
	out:remove()
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
Tool.Activated:connect(onActivated)</ProtectedString>
