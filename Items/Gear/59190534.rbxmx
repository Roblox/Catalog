&#9;&#9;&#9;&#9;&#9;everypart[v]=dist
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;getpartranges(v)
&#9;&#9;end
&#9;end
end
