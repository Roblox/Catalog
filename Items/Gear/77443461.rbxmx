		<Item class="Script" referent="RBX7">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">SwordScript</string>
				<ProtectedString name="Source">function waitForChild(parent, instance)
&#9;while parent:FindFirstChild(instance) == nil do print(instance) wait() end 
&#9;return parent:FindFirstChild(instance)
end 

local Tool = script.Parent 
local Sword = Tool.Handle 

local vCharacter
local vPlayer
local vTorso
local vHumanoid 

Tool.Enabled = true 

local Glow_BoolValue = waitForChild(Tool, &quot;Glow&quot;)
local Light_Weld = waitForChild(Tool, &quot;LightWeld&quot;)  
local Fire_Part = waitForChild(Sword, &quot;FirePart&quot;)
local Light_Fire = waitForChild(Fire_Part, &quot;LightFire&quot;) 

local Damage = 15

local RBX_Debris = game:GetService(&quot;Debris&quot;)

local TimeOfDay 

function blow(hit)
&#9;if hit and hit.Parent then 
&#9;&#9;local humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;if hit.Parent:IsA(&quot;Hat&quot;) then
&#9;&#9;&#9;humanoid = hit.Parent.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;end 
&#9;&#9;if humanoid and humanoid ~= vHumanoid and vHumanoid then 
&#9;&#9;&#9;humanoid:TakeDamage(Damage)
&#9;&#9;&#9;if Light_Fire and Light_Fire.Enabled == true then
&#9;&#9;&#9;&#9;coroutine.resume(coroutine.create(function()
&#9;&#9;&#9;&#9;&#9;if humanoid and humanoid.Parent and humanoid.Parent:FindFirstChild(&quot;Torso&quot;) and not humanoid.Parent.Torso:FindFirstChild(&quot;LightFire&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;local cFire = Light_Fire:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;cFire.Parent = humanoid.Parent:FindFirstChild(&quot;Torso&quot;) 
&#9;&#9;&#9;&#9;&#9;&#9;RBX_Debris:AddItem(cFire, 4) 
&#9;&#9;&#9;&#9;&#9;&#9;local duration = 4
&#9;&#9;&#9;&#9;&#9;&#9;while duration &gt; 0 do
&#9;&#9;&#9;&#9;&#9;&#9;&#9;humanoid:TakeDamage(5)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;duration = duration - 1 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;wait(1.0) 
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;end)) 
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;end 
end 

function onEquipped() 
&#9;local time = {}

&#9;TimeOfDay = game.Lighting.TimeOfDay 

&#9;for t in string.gmatch(game.Lighting.TimeOfDay, &quot;%d+&quot;) do &#9;&#9;
&#9;&#9;time[#time + 1] = t
&#9;end 

&#9;local hours = time[1]

&#9;if tonumber(hours) &gt; 6 and tonumber(hours) &lt; 18 and Light_Fire then 
&#9;&#9;Damage = 27
&#9;&#9;Light_Fire.Enabled = true 
&#9;else 
&#9;&#9;Damage = 15
&#9;&#9;Light_Fire.Enabled = false
&#9;end 
&#9;if Light_Weld then 
&#9;&#9;Light_Weld.Parent = Tool 
&#9;end 
&#9;
&#9;vCharacter = Tool.Parent 
&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;vHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;vTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
end 

function onUnequipped()
&#9;--if TimeOfDay then game.Lighting.TimeOfDay = TimeOfDay  end 
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
Sword.Touched:connect(blow)

function shootLightRay()
&#9;local time = {}

&#9;TimeOfDay = game.Lighting.TimeOfDay 

&#9;for t in string.gmatch(game.Lighting.TimeOfDay, &quot;%d+&quot;) do &#9;&#9;
&#9;&#9;time[#time + 1] = t
&#9;end 

&#9;local hours = time[1]
&#9;local minutes = time[2] 

&#9;local ray = Instance.new(&quot;Part&quot;)
&#9;ray.Parent = game.Workspace
&#9;ray.FormFactor = 3 
&#9;ray.Anchored = true
&#9;ray.CanCollide = false 
&#9;ray.Size = Vector3.new(1, 0.2, 1)
&#9;ray.Material = Enum.Material.Plastic
&#9;ray.BrickColor = BrickColor.new(&quot;Institutional white&quot;)
&#9;ray.Locked = true 
&#9;ray.CFrame = CFrame.new(Fire_Part.CFrame.p, Vector3.new(0, 1, 0)) 
&#9;RBX_Debris:AddItem(ray, 1.5)

&#9;-- We don&apos;t care about the seconds 
&#9;-- Make it MidNight 

&#9;local midNightHour = 13 
&#9;local midNightMinutes = 0 
&#9;-- Takes 10 steps 

&#9;if tonumber(hours) == 0 then hours = 24 end 
&#9;--if tonumber(minutes) == 0 then minutes = 60 end 
&#9;
&#9;local hoursPerStep = midNightHour/10 
&#9;local minutesPerStep =  midNightMinutes/10 
&#9;
&#9;while ray.Size.Y &lt; 200 do &#9;&#9;
&#9;&#9;ray.Size = ray.Size *  Vector3.new(1, 2, 1) 
&#9;&#9;ray.CFrame = CFrame.new(Fire_Part.CFrame.p) * CFrame.new(0, 0, -ray.Size/2)&#9;&#9;
&#9;&#9;wait(0.1)
&#9;end 

&#9;local numSteps = 10 
&#9;
&#9;while numSteps &gt; 1 do 
&#9;&#9;hours = hours + hoursPerStep
&#9;&#9;minutes = minutes + minutesPerStep
&#9;&#9;if tonumber(hours) &gt; 24 then 
&#9;&#9;&#9;hours = hours%24
&#9;&#9;end 
&#9;&#9;print(hours, minutes)
&#9;&#9;if tonumber(hours) &gt; 0 and tonumber(minutes) &gt; 0 then 
&#9;&#9;&#9;game.Lighting.TimeOfDay = hours .. &quot;:&quot; .. minutes .. &quot;:&quot; .. &quot;00&quot;  
&#9;&#9;end 
&#9;&#9;numSteps = numSteps - 1
&#9;&#9;wait(0.1) 
&#9;end 
end 

function checkForPower()
&#9;local duration = 30 &#9;
&#9;while duration &gt; 0 do 
&#9;&#9;if not game.Workspace:FindFirstChild(&quot;Bloom&quot;) then
&#9;&#9;&#9;duration = 0
&#9;&#9;end 
&#9;&#9;duration = duration - wait() 
&#9;&#9;wait() 
&#9;end 
end 

Glow_BoolValue.Changed:connect(function()
&#9;local wither = game.Workspace:FindFirstChild(&quot;Wither&quot;) 
&#9;local bloom = game.Workspace:FindFirstChild(&quot;Bloom&quot;)
&#9;
&#9;if Glow_BoolValue.Value == true and not bloom and vHumanoid then
&#9;&#9;Light_Fire.Enabled = true 
&#9;&#9;Damage = 27
&#9;&#9;vHumanoid.WalkSpeed = 0.0 
&#9;&#9;bloom = Instance.new(&quot;BoolValue&quot;)
&#9;&#9;bloom.Name = &quot;Bloom&quot;
&#9;&#9;bloom.Parent = game.Workspace 
&#9;&#9;RBX_Debris:AddItem(wither, 30) 
&#9;&#9;shootLightRay() 
&#9;&#9;vHumanoid.WalkSpeed = 21.0
&#9;&#9;if wither then wither:Destroy() end 
&#9;&#9;coroutine.resume(coroutine.create(checkForPower))
&#9;&#9;wait(30)
&#9;&#9;Damage = 15
&#9;&#9;vHumanoid.WalkSpeed = 16.0
&#9;&#9;Light_Fire.Enabled = false 
&#9;&#9;--game.Lighting.TimeOfDay = TimeOfDay 
&#9;&#9;wait(15)
&#9;&#9;Glow_BoolValue.Value = false
&#9;else &#9;&#9;
&#9;&#9;if not vHumanoid then 
&#9;&#9;&#9;if Glow_BoolValue then 
&#9;&#9;&#9;&#9;Glow_BoolValue.Value = false 
&#9;&#9;&#9;end 
&#9;&#9;end  
&#9;end  
end) 



</ProtectedString>
			</Properties>
		</Item>
		<Item class="LocalScript" referent="RBX8">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">AnimateScript</string>
				<ProtectedString name="Source">local Tool = script.Parent 
local Sword = Tool.Handle

local vCharacter
local vPlayer 
local vTorso 
local vHumanoid 

local RBX_Debris = game:GetService(&quot;Debris&quot;)

local Slash_Animation
local Stab_Animation
local Lighten_Animation 

local Charge_Animation 

local Slash_Sound 

local MouseDown_Bool = false 

local Camera = game.Workspace.CurrentCamera

local Glow_BoolValue 
local Animating_Bool = false 

local function onEquipped(mouse)
&#9;vCharacter = Tool.Parent 
&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;vTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;vHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;) 

&#9;mouse.Button1Down:connect(function()
&#9;&#9;if Animating_Bool == false then 
&#9;&#9;&#9;MouseDown_Bool = true 
&#9;&#9;&#9;Charge_Animation = vHumanoid:LoadAnimation(Tool.ChargeAnim)
&#9;&#9;&#9;Charge_Animation:Play() 
&#9;&#9;&#9;local currentSpeed = vHumanoid.WalkSpeed
&#9;&#9;&#9;while MouseDown_Bool do &#9;
&#9;&#9;&#9;&#9;if game.Workspace:FindFirstChild(&quot;Bloom&quot;) then
&#9;&#9;&#9;&#9;&#9;vHumanoid:TakeDamage(-0.3) -- Heals 6 hp per second while you are charging 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;wait() 
&#9;&#9;&#9;end 
&#9;&#9;&#9;if Charge_Animation then Charge_Animation:Stop() end 
&#9;&#9;&#9;local vDirection = (vTorso.Position - Camera.CoordinateFrame.p).unit 
&#9;&#9;&#9;local mDirection = (vHumanoid.TargetPoint - Camera.CoordinateFrame.p).unit 

&#9;&#9;&#9;local angleBetweenVectors = math.abs(math.acos(vDirection:Dot(mDirection)))&#9;&#9;&#9;
&#9;&#9;&#9;if angleBetweenVectors &lt; 0.5 then 
&#9;&#9;&#9;&#9;Slash_Animation = vHumanoid:LoadAnimation(Tool.SlashAnim)
&#9;&#9;&#9;&#9;Slash_Animation:Play() &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;Stab_Animation = vHumanoid:LoadAnimation(Tool.StabAnim)
&#9;&#9;&#9;&#9;Stab_Animation:Play() 
&#9;&#9;&#9;&#9;Tool.Grip = CFrame.new(0, 0, -2, 1, 0, 0, 0, -1, 0, 0, 0, -1)
&#9;&#9;&#9;&#9;wait(1.3)
&#9;&#9;&#9;&#9;Tool.Grip = CFrame.new(0, 0, -2, 0, 0, 1, 1, 0, 0, 0, 1, 0)
&#9;&#9;&#9;end &#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;vHumanoid.WalkSpeed = currentSpeed 
&#9;&#9;else &#9;&#9;&#9;&#9;&#9;
&#9;&#9;end 
&#9;end) 

&#9;mouse.Button1Up:connect(function()
&#9;&#9;MouseDown_Bool = false 
&#9;&#9;if Animating_Bool == false then 
&#9;&#9;&#9;Tool.Enabled = false
&#9;&#9;&#9;Animating_Bool = true 
&#9;&#9;&#9;wait(1.5)&#9;&#9;&#9;
&#9;&#9;&#9;Tool.Enabled = true 
&#9;&#9;&#9;Animating_Bool = false &#9;&#9;
&#9;&#9;end &#9;&#9;
&#9;end)

&#9;mouse.KeyDown:connect(function(key)
&#9;&#9;key = key:lower() 
&#9;&#9;if key == &quot;q&quot; then 
&#9;&#9;&#9;Glow_BoolValue = Tool:FindFirstChild(&quot;Glow&quot;)
&#9;&#9;&#9;&#9;local time = {}
&#9;&#9;&#9;&#9;TimeOfDay = game.Lighting.TimeOfDay 
&#9;&#9;&#9;&#9;for t in string.gmatch(game.Lighting.TimeOfDay, &quot;%d+&quot;) do &#9;&#9;
&#9;&#9;&#9;&#9;&#9;time[#time + 1] = t
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;local hours = time[1]
&#9;&#9;&#9;&#9;local minutes = time[2] 
&#9;&#9;&#9;if Glow_BoolValue and Glow_BoolValue.Value == false and (tonumber(hours) &gt; 18 or tonumber(hours) &lt; 5) then 
&#9;&#9;&#9;&#9;Animating_Bool = true 
&#9;&#9;&#9;&#9;Lighten_Animation = vHumanoid:LoadAnimation(Tool.LightenAnim)
&#9;&#9;&#9;&#9;Lighten_Animation:Play() &#9;
&#9;&#9;&#9;&#9;wait(0.4)
&#9;&#9;&#9;&#9;vHumanoid.WalkSpeed = 0.0 
&#9;&#9;&#9;&#9;Glow_BoolValue.Value = true&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;wait(2.0)
&#9;&#9;&#9;&#9;Animating_Bool = false 
&#9;&#9;&#9;end  
&#9;&#9;end 
&#9;end) 
end 

local function onUnequipped()
&#9;-- Do stuff
&#9;if Charge_Animation  then Charge_Animation:Stop() end 
&#9;if Slash_Animation then Slash_Animation:Stop() end 
&#9;if Stab_Animation then Stab_Animation:Stop() end 
&#9;if Lighten_Animation then Lighten_Animation:Stop() end 
&#9;Animating_Bool = false &#9;
&#9;Tool.Grip = CFrame.new(0, 0, -2, 0, 0, 1, 1, 0, 0, 0, 1, 0)
&#9;MouseDown_Bool = false 
&#9;vHumanoid.WalkSpeed = 16.0
end 

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)</ProtectedString>
