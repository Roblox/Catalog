				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">LocalSwordScript</string>
				<ProtectedString name="Source">-- Copied from Grimgold Periastron Beta

local Tool = script.Parent
local Handle = Tool.Handle

local vCharacter
local myTorso
local myHumanoid

Tool.Enabled = true 

local anims = {Tool:WaitForChild(&quot;RightSlash&quot;), Tool:WaitForChild(&quot;Slash&quot;), Tool:WaitForChild(&quot;SlashAnim&quot;), Tool:WaitForChild(&quot;SlashAnim&quot;), Tool:WaitForChild(&quot;SlashAnim&quot;)}
local slashAnim
local holdAnim

local LightsOutSound

local swing 

local lastAttack = 0

local DARKNESS_LOCK_TIME = 30
local TRANSPARENT_TIME = 15
local DARKNESS_SPARKLES_TIME = 1

local r = game:service(&quot;RunService&quot;)

function lunge(humanoid, torso) &#9;
&#9;local holdAnim = humanoid:LoadAnimation(Tool.RightSlash)
&#9;if holdAnim then 
&#9;&#9;holdAnim:Play() 
&#9;else 
&#9;&#9;Tool.Enabled = true
&#9;&#9;return 
&#9;end
&#9;local target = humanoid.TargetPoint 
&#9;local direction = (target - torso.Position) * Vector3.new(1, 0, 1) 
&#9;newBV = nil &#9;
&#9;if direction.magnitude &gt; .01 then            
&#9;&#9;direction = direction.unit 
&#9;&#9;newBV = Instance.new(&quot;BodyVelocity&quot;) 
&#9;&#9;newBV.P = 100000 
&#9;&#9;newBV.maxForce = Vector3.new(newBV.P, 0, newBV.P) 
&#9;&#9;newBV.velocity = direction * 50 
&#9;&#9;newBV.Parent = torso 
&#9;&#9;torso.CFrame = CFrame.new(torso.Position, target * Vector3.new(1, 0, 1) + Vector3.new(0, torso.Position.Y, 0)) 
&#9;end 
&#9;wait(.75)       
&#9;if newBV ~= nil then newBV:remove() end 
&#9;wait(.5) 
&#9;
end 

function lightsOut(key)
&#9;if key:lower() == &quot;q&quot; and not Tool:FindFirstChild(&apos;DarknessDisabled&apos;) and not game.Lighting:FindFirstChild(&quot;BlackPeriastronWasHere&quot;) and vCharacter then
&#9;&#9;LightsOutSound = Tool:FindFirstChild(&apos;LightsOutSound&apos;)
&#9;&#9;if LightsOutSound then LightsOutSound:Play() end
&#9;&#9;-- Create tag in lighting so other players with this gear can&apos;t mess it up
&#9;&#9;LightingTag = Instance.new(&quot;BoolValue&quot;)
&#9;&#9;LightingTag.Name = &quot;BlackPeriastronWasHere&quot;
&#9;&#9;LightingTag.Value = true
&#9;&#9;LightingTag.Parent = game.Lighting
&#9;&#9;
&#9;&#9;-- Remove sparkles, make handle invisible...
&#9;&#9;local sparkles = Handle:FindFirstChild(&apos;Sparkles&apos;)
&#9;&#9;if sparkles then sparkles.Parent = nil end
&#9;&#9;
&#9;&#9;-- Lock special attack so you can&apos;t use it multiple times in succession.
&#9;&#9;local lock = Instance.new(&apos;BoolValue&apos;)
&#9;&#9;lock.Name = &apos;DarknessDisabled&apos;
&#9;&#9;lock.Value = true
&#9;&#9;lock.Parent = Tool
&#9;&#9;game:GetService(&apos;Debris&apos;):AddItem(lock, DARKNESS_LOCK_TIME)
&#9;&#9;
&#9;&#9;-- Make sparkles reset when darkness can be used again&#9;&#9;
&#9;&#9;Tool.ChildRemoved:connect(function(child)
&#9;&#9;&#9;if child.Name == &apos;DarknessDisabled&apos; then
&#9;&#9;&#9;&#9;if sparkles then sparkles.Parent = Handle end
&#9;&#9;&#9;end
&#9;&#9;end)

&#9;&#9;-- Create a new part to hold the light
&#9;&#9;local lightHolder = Instance.new(&apos;Part&apos;)
&#9;&#9;lightHolder.CanCollide = false
&#9;&#9;lightHolder.Anchored = true
&#9;&#9;lightHolder.Transparency = 1.0
&#9;&#9;lightHolder.Position = Handle.Position
&#9;&#9;lightHolder.Name = &apos;BlackPeriastronLightHolder&apos;
&#9;&#9;lightHolder.Parent = Workspace
&#9;&#9;
&#9;&#9;-- Script to absorb light is in workspace so it can be reset even if 
&#9;&#9;-- this character dies
&#9;&#9;local lightsOutScript = Tool:WaitForChild(&apos;LightsOutScript&apos;):Clone()
&#9;&#9;lightsOutScript.LightHolder.Value = lightHolder
&#9;&#9;lightsOutScript.Handle.Value = Handle
&#9;&#9;lightsOutScript.Parent = Workspace
&#9;&#9;lightsOutScript.Disabled = false
&#9;&#9;
&#9;&#9;-- But script to make character invisble just goes under the character.
&#9;&#9;local ninjafyScript = Tool:WaitForChild(&apos;Ninjafy&apos;):Clone()
&#9;&#9;ninjafyScript.Parent = vCharacter
&#9;&#9;ninjafyScript.Disabled = false
&#9;end 
end 

function onActivated()
&#9;if Tool.Enabled == false then 
&#9;&#9;return 
&#9;end

&#9;vCharacter = Tool.Parent
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;
&#9;if myTorso == nil or myHumanoid == nil then 
&#9;&#9;return 
&#9;end
&#9;
&#9;-- Add sparkles for visibility if it&apos;s dark
&#9;if Handle.Transparency &gt; 0.5 then
&#9;&#9;local newSparkles = Instance.new(&quot;Sparkles&quot;)
&#9;&#9;newSparkles.SparkleColor = Color3.new(0,0,0)
&#9;&#9;newSparkles.Parent = Handle
&#9;&#9;game:GetService(&apos;Debris&apos;):AddItem(newSparkles, DARKNESS_SPARKLES_TIME)
&#9;end

&#9;Tool.Enabled = false
&#9;tick()
&#9;local num = anims[math.random(1, #anims)]
&#9;print(num)
&#9;local t = r.Stepped:wait()
&#9;if (t - lastAttack &lt; 0.2) then&#9;&#9;
&#9;&#9;lunge(myHumanoid, myTorso)
&#9;else
&#9;&#9;slashAnim = myHumanoid:LoadAnimation(num)
&#9;&#9;if slashAnim then slashAnim:Play() end
&#9;end
&#9;lastAttack = t&#9;
&#9;
&#9;Tool.Enabled = true
end

function onEquipped(mouse)&#9;
&#9;if mouse then 
&#9;&#9;mouse.KeyDown:connect(lightsOut)
&#9;end
&#9;vCharacter = Tool.Parent
end

function onUnequipped()
&#9;if slashAnim then slashAnim:Stop() end
&#9;if holdAnim then holdAnim:Play() end 
&#9;if LightsOutSound then LightsOutSound:Stop() end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
</ProtectedString>
