				<ProtectedString name="Source">--Stickmasterluke


sp=script.Parent


function waitfor(parent,name)
&#9;while parent:FindFirstChild(name)==nil do
&#9;&#9;wait()
&#9;end
&#9;return parent:FindFirstChild(name)
end



local h=waitfor(sp.Parent,&quot;Humanoid&quot;)

for i=1,10 do
&#9;h.Sit=true
&#9;wait(1)
end

sp:remove()
</ProtectedString>
