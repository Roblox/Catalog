&#9;if Gui.Parent then return end 
&#9;Gui = Instance.new(&apos;ScreenGui&apos;)
&#9;Gui.Parent = WaitForChild(Player, &apos;PlayerGui&apos;)

&#9;local sText = Instance.new(&apos;TextLabel&apos;)
