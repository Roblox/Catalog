&#9;&#9;&#9;&#9;&#9;&#9;return 
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;PurgeAnim = Humanoid:LoadAnimation(Tool.PurgeAnim)
&#9;&#9;&#9;&#9;&#9;PurgeAnim:Play() 
&#9;&#9;&#9;&#9;&#9;if PurgeSound then PurgeSound:Play() end 
&#9;&#9;&#9;&#9;&#9;purge.Value = true 
&#9;&#9;&#9;&#9;end &#9;
&#9;&#9;&#9;elseif key == &apos;q&apos; then 
&#9;&#9;&#9;&#9;local decimate = WaitForChild(Tool, &apos;Decimate&apos;)
&#9;&#9;&#9;&#9;if decimate.Value == false then 
&#9;&#9;&#9;&#9;&#9;decimate.Value = true 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end 
&#9;&#9;end)
&#9;end 
end

local function OnUnequipped()
