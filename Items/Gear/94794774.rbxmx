local function WaitForChild(parent, child)
&#9;while parent:FindFirstChild(child) == nil do 
&#9;&#9;print(parent, child)
&#9;&#9;wait()
&#9;end 
&#9;return parent[child]
end 

local Player 
local Character 
local Torso 
local Humanoid 

local Tool = script.Parent 
local Sword = Tool.Handle 

local LastAttack = 0

local FireWeld = WaitForChild(Tool, &apos;FireWeld&apos;)
local FirePart = WaitForChild(Sword, &apos;FirePart&apos;)
local SoulFire = WaitForChild(FirePart, &apos;Fire&apos;)

SoulFire.Size = 2.5

local RunService = Game:GetService(&apos;RunService&apos;)
local DebrisService = Game:GetService(&apos;Debris&apos;)

local Lunging  = false 

local TrailPart = Instance.new(&apos;Part&apos;)
TrailPart.FormFactor = 3
TrailPart.Size = Vector3.new(1, 1, 1)
TrailPart.Anchored = true 
TrailPart.CanCollide = false 
TrailPart.Transparency = 1.0 

local Fire = Instance.new(&apos;Fire&apos;)
Fire.Name = &apos;TrailFire&apos;
Fire.Heat = 25 
Fire.Size = 4
Fire.Parent = TrailPart 

local EffectPart = Instance.new(&apos;Part&apos;)
EffectPart.FormFactor =3
EffectPart.Size = Vector3.new(2, 2, 2)
EffectPart.Name = &apos;WingCutter&apos;

-- TODO: Add the mesh and texture IDs 
local EffectMesh = Instance.new(&apos;SpecialMesh&apos;)
EffectMesh.Scale = Vector3.new(1, 1, 1)
EffectMesh.Parent = EffectPart

local EffectFire = Instance.new(&apos;Fire&apos;)
EffectFire.Heat = 25 
EffectFire.Size = 5.0
EffectFire.Parent = EffectPart

local SwordDamage = 25

Tool.Enabled = true 

local TrailDamage = 5 -- move to separate script perhaps? 

local PurgeValue 
local ConsumeValue 

local SoulMark = Instance.new(&apos;BoolValue&apos;)
SoulMark.Name = &apos;SoulMark&apos;

local Equipped = false 

local SoulStealerValue

local Souls 

local function blow(hitPart, damage, part)
&#9;if hitPart and hitPart.Parent then
&#9;&#9;local humanoid = hitPart.Parent:FindFirstChild(&apos;Humanoid&apos;)
&#9;&#9;if hitPart.Parent:IsA(&apos;Hat&apos;) and hitPart.Parent.Parent then 
&#9;&#9;&#9;humanoid = hitPart.Parent.Parent:FindFirstChild(&apos;Humanoid&apos;)
&#9;&#9;end 
&#9;&#9;if humanoid and Humanoid and humanoid ~= Humanoid then&#9;&#9;
&#9;&#9;&#9;if humanoid.Health - SwordDamage &lt; 20 and SoulFire.Enabled and not humanoid.Parent:FindFirstChild(&apos;DecimateScript&apos;) then 
&#9;&#9;&#9;&#9;local dScript = script.DecimateScript:Clone()
&#9;&#9;&#9;&#9;local tValue = WaitForChild(dScript, &apos;Torso&apos;)
&#9;&#9;&#9;&#9;tValue.Value = Torso 
&#9;&#9;&#9;&#9;dScript.Parent = humanoid.Parent
&#9;&#9;&#9;&#9;dScript.Disabled = false 
&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;humanoid:TakeDamage(damage)
&#9;&#9;&#9;&#9;Humanoid:TakeDamage(-damage/10)
&#9;&#9;&#9;end 
&#9;&#9;&#9;if humanoid.Parent:FindFirstChild(&apos;Torso&apos;) then 
&#9;&#9;&#9;&#9;-- SLOW THEM DOWN, add a mark &#9;&#9;&#9;
&#9;&#9;&#9;&#9;local mark = SoulMark:Clone()
&#9;&#9;&#9;&#9;mark.Parent = humanoid.Parent
&#9;&#9;&#9;&#9;DebrisService:AddItem(mark, 5.75)

&#9;&#9;&#9;&#9;Spawn(function()
&#9;&#9;&#9;&#9;&#9;humanoid.WalkSpeed = 9.0
&#9;&#9;&#9;&#9;&#9;wait(1.25)
&#9;&#9;&#9;&#9;&#9;humanoid.WalkSpeed = 16.0&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;end)
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;end 
end 

local function Lunge()&#9;

&#9;Lunging = true 

&#9;local target = Humanoid.TargetPoint 
&#9;local direction = (target - Torso.Position) * Vector3.new(1, 0, 1) 
&#9;local newBV = nil 

&#9;Tool.Grip = CFrame.new(0, -2.3, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)

&#9;if direction.magnitude &gt; .01 then 
&#9;&#9;direction = direction.unit 
&#9;&#9;newBV = Instance.new(&quot;BodyVelocity&quot;) 
&#9;&#9;newBV.P = 100000 
&#9;&#9;newBV.maxForce = Vector3.new(newBV.P, 0, newBV.P) 
&#9;&#9;newBV.velocity = direction * 50 
&#9;&#9;newBV.Parent = Torso
&#9;&#9;Torso.CFrame = CFrame.new(Torso.Position, target * Vector3.new(1, 0, 1) + Vector3.new(0, Torso.Position.Y, 0)) 
&#9;end 
&#9;Spawn(function() 
&#9;&#9;while Lunging do 
&#9;&#9;&#9;local hPart = Sword:Clone()
&#9;&#9;&#9;hPart.Transparency = 0.5 
&#9;&#9;&#9;hPart.Anchored = true 

&#9;&#9;&#9;hPart.Parent = Game.Workspace 

&#9;&#9;&#9;hPart.Touched:connect(function(hitPart) blow(hitPart, 5.0, hPart) end)

&#9;&#9;&#9;DebrisService:AddItem(hPart, 1)

&#9;&#9;&#9;wait(0.08)
&#9;&#9;end 
&#9;end)
&#9;wait(0.75) 
&#9;if newBV ~= nil then 
&#9;newBV:Destroy() 
&#9;end 
&#9;Lunging = false 
&#9;Tool.Grip = CFrame.new(0, -2.3, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
&#9;wait(0.5)      
end

local function Attack()
&#9;-- do stuff here &#9;&#9;
&#9;Tool.Grip = CFrame.new(0, -2.29999995, 0, 0, 1, -0, 0, -0, -1, -1, 0, -0)
&#9;local anim = Instance.new(&quot;StringValue&quot;) 
&#9;anim.Name = &quot;toolanim&quot; 
&#9;anim.Value = &quot;Slash&quot; 
&#9;anim.Parent = Tool 
&#9;Spawn(function()
&#9;&#9;local count = 3
&#9;&#9;while count &gt; 0 do  
&#9;&#9;&#9;local hPart = Sword:Clone()
&#9;&#9;&#9;hPart.Transparency = 0.5 
&#9;&#9;&#9;hPart.Anchored = true 

&#9;&#9;&#9;hPart.Parent = Game.Workspace 

&#9;&#9;&#9;DebrisService:AddItem(hPart, 0.35)
&#9;&#9;&#9;count = count - 1
&#9;&#9;&#9;wait(0.06)
&#9;&#9;end 
&#9;end)
&#9;Tool.Grip = CFrame.new(0, -2.3, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)

end

local function OnActivated()&#9;
&#9;if not Humanoid or not Torso then 
&#9;&#9;return 
&#9;end 

&#9;if not Tool.Enabled then  
&#9;&#9;return 
&#9;end 

&#9;Tool.Enabled = false 

&#9;print(timestep, LastAttack)
&#9;local timestep = RunService.Stepped:wait() 
     if (timestep - LastAttack &lt; 0.2) then &#9;&#9;
&#9;&#9;Lunge()      &#9;
&#9;else 
&#9;&#9;Attack()
     end 

     LastAttack = timestep

     Tool.Enabled = true      
end

local function SetPlayerMaxHealth(value)
&#9;local ratio = Humanoid.Health/Humanoid.MaxHealth

&#9;Humanoid.MaxHealth = value 

&#9;while Humanoid.Health &lt; ratio * Humanoid.MaxHealth do 
&#9;&#9;Humanoid.Health = ratio * Humanoid.MaxHealth
&#9;&#9;wait()
&#9;end 
end

local function Encompass()
&#9;local SoulColors = {
&#9;&#9;&#9;&#9;&#9;&#9;Color3.new(1, 0, 0),
&#9;&#9;&#9;&#9;&#9;&#9;Color3.new(1, 0.2, 0),
&#9;&#9;&#9;&#9;&#9;&#9;Color3.new(0.8, 0.2, 0),
&#9;&#9;&#9;&#9;&#9;&#9;Color3.new(1, 0, 0.2),
&#9;&#9;&#9;&#9;&#9;&#9;Color3.new(0.8, 0.4, 0)
&#9;&#9;&#9;&#9;&#9;}
&#9;Souls:ClearAllChildren() 
&#9;if SoulStealerValue then 
&#9;&#9;for i = 1, SoulStealerValue.Value do 
&#9;&#9;&#9;local cPart = Instance.new(&apos;Part&apos;)
&#9;&#9;&#9;cPart.FormFactor = 3
&#9;&#9;&#9;cPart.Size = Vector3.new(0.5, 0.5, 0.5)
&#9;&#9;&#9;cPart.Transparency = 1.0
&#9;&#9;&#9;cPart.Anchored = true 

&#9;&#9;&#9;local cFire = SoulFire:Clone()
&#9;&#9;&#9;cFire.Parent = cPart 
&#9;&#9;&#9;cFire.Enabled = true 
&#9;&#9;&#9;cFire.Size = 1
&#9;&#9;&#9;cFire.Color = SoulColors[math.random(1, #SoulColors)]

&#9;&#9;&#9;cPart.Parent = Souls 

&#9;&#9;&#9;DebrisService:AddItem(cPart, 45.0)

&#9;&#9;&#9;Spawn(function()
&#9;&#9;&#9;&#9;while cPart and cPart.Parent do 
&#9;&#9;&#9;&#9;&#9;for i = 1, 30 do 
&#9;&#9;&#9;&#9;&#9;&#9;cPart.CFrame = (Torso.CFrame + CFrame.Angles(0, 2 * i * math.pi/30, 0) * Torso.CFrame.lookVector * 5.0)
&#9;&#9;&#9;&#9;&#9;&#9;wait() 
&#9;&#9;&#9;&#9;&#9;end &#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end)

&#9;&#9;&#9;wait(0.75)
&#9;&#9;end 
&#9;end 
end 

local function OnEquipped()
&#9;Character = Tool.Parent
&#9;Player = Game.Players:GetPlayerFromCharacter(Character)
&#9;Torso = WaitForChild(Character, &apos;Torso&apos;)
&#9;Humanoid = WaitForChild(Character, &apos;Humanoid&apos;)

&#9;Humanoid.WalkSpeed = 18.0

&#9;Equipped = true 

&#9;SetPlayerMaxHealth(135)

&#9;-- Provides increased regeneration 
&#9;Spawn(function()
&#9;&#9;while Equipped do 
&#9;&#9;&#9;if Humanoid.Health &lt; Humanoid.MaxHealth then 
&#9;&#9;&#9;&#9;Humanoid.Health = Humanoid.Health + 0.25
&#9;&#9;&#9;end 
&#9;&#9;&#9;wait(1.0)
&#9;&#9;end 
&#9;end)

&#9;if FireWeld then 
&#9;&#9;FireWeld.Parent = Sword 
&#9;end 

&#9;if not Character:FindFirstChild(&apos;SoulStealer&apos;) then 
&#9;&#9;SoulStealerValue = Instance.new(&apos;IntValue&apos;)
&#9;&#9;SoulStealerValue.Name = &apos;SoulStealer&apos;
&#9;&#9;SoulStealerValue.Parent = Character 

&#9;&#9;SoulStealerValue.Changed:connect(Encompass)
&#9;end 

&#9;if not Character:FindFirstChild(&apos;Souls&apos;) then 
&#9;&#9;Souls = Instance.new(&apos;Model&apos;)
&#9;&#9;Souls.Name = &apos;Souls&apos;
&#9;&#9;Souls.Parent = Character 
&#9;end 
end

local function OnUnequipped()
&#9;Tool.Grip = CFrame.new(0, -2.3, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
&#9;Equipped = false 
&#9;SetPlayerMaxHealth(100)
&#9;Humanoid.WalkSpeed = 16.0
end 

Tool.Equipped:connect(OnEquipped)
Tool.Activated:connect(OnActivated)
Sword.Touched:connect(function(hitPart) blow(hitPart, SwordDamage) end)

ConsumeValue = WaitForChild(Tool, &apos;Consume&apos;)
PurgeValue = WaitForChild(Tool, &apos;Purge&apos;)
DecimateValue = WaitForChild(Tool, &apos;Decimate&apos;)

local function ConsumeTarget(character, torso, num)
&#9;local cPart = Instance.new(&apos;Part&apos;)
&#9;cPart.FormFactor = 3
&#9;cPart.Size = Vector3.new(0.5, 0.5, 0.5)
&#9;cPart.Transparency = 1.0

&#9;local cFire = SoulFire:Clone()
&#9;cFire.Parent = cPart 
&#9;cFire.Enabled = true 
&#9;cFire.Size = 1

&#9;if num and num &lt; 3 then 
&#9;&#9;num = 3
&#9;end 

&#9;if character and torso then 
&#9;&#9;num = num or 4
&#9;&#9;for i = 1, num do &#9;&#9;&#9;
&#9;&#9;&#9;local fPart = cPart:Clone() 
&#9;&#9;&#9;fPart.CFrame = Torso.CFrame + Torso.CFrame.lookVector * 4.0 
&#9;&#9;&#9;local rPropulsion = Instance.new(&apos;RocketPropulsion&apos;)
&#9;&#9;&#9;rPropulsion.Target = torso
&#9;&#9;&#9;rPropulsion.TargetOffset = Vector3.new(0, 2, 0)

&#9;&#9;&#9;rPropulsion.Thrust = 50000
&#9;&#9;&#9;rPropulsion.ThrustP = 500
&#9;&#9;&#9;rPropulsion.ThrustD = 5

&#9;&#9;&#9;rPropulsion.MaxSpeed = 75

