&#9;PurgeSound = WaitForChild(Sword, &apos;PurgeSound&apos;)
&#9;ConsumeSound = WaitForChild(Sword, &apos;ConsumeSound&apos;)
&#9;if mouse then 
&#9;&#9;mouse.KeyDown:connect(function (key)
&#9;&#9;&#9;key = key:lower() 
&#9;&#9;&#9;if key == &apos;e&apos; then &#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;local soulConsume = WaitForChild(Tool, &apos;Consume&apos;)
&#9;&#9;&#9;&#9;if soulConsume.Value == false then 
&#9;&#9;&#9;&#9;&#9;ConsumeAnim = Humanoid:LoadAnimation(Tool.ConsumeAnim)
&#9;&#9;&#9;&#9;&#9;--ConsumeAnim:Play() 
&#9;&#9;&#9;&#9;&#9;if ConsumeSound then ConsumeSound:Play() end 
&#9;&#9;&#9;&#9;&#9;soulConsume.Value = true 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;return 
&#9;&#9;&#9;elseif key == &apos;x&apos; then 
&#9;&#9;&#9;&#9;local purge = WaitForChild(Tool, &apos;Purge&apos;)
&#9;&#9;&#9;&#9;if purge.Value == false then 
&#9;&#9;&#9;&#9;&#9;local souls = Character:FindFirstChild(&apos;Souls&apos;)
&#9;&#9;&#9;&#9;&#9;if souls and #souls:GetChildren() == 0 then 
