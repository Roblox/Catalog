				<string name="Name">BookScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Book = Tool.Handle

Tool.Enabled = true
local vCharacter

local leftHandWeld
local bookClone
local spark

local bookSound 

-- Coroutines
local spikeCo
local ropeCo
local prisonCo

local rope
local prison
local spike

local ropeTextRef
local prisonTextRef
local spikeTextRef

local sgRef
local frameRef
local prisonBtnRef
local ropeBtnRef
local spikeBtnRef

local ropeDamage = 30
local spikeDamage = 30
local fireballs = {}

local spikeClone

local debris = game:GetService(&quot;Debris&quot;)

local spikePart = Instance.new(&quot;Part&quot;)
spikePart.FormFactor = 3
spikePart.Size = Vector3.new(1, 2, 1)
spikePart.Shape = &quot;Block&quot;
spikePart.Name = &quot;Spike&quot;
spikePart.CanCollide = false

local spikeMesh = Instance.new(&quot;SpecialMesh&quot;)
spikeMesh.MeshId = &quot;http://www.roblox.com/asset/?id=59092052&quot;
spikeMesh.TextureId = &quot;http://www.roblox.com/asset/?id=59801821&quot;
spikeMesh.Parent = spikePart

ropePart = Instance.new(&quot;Part&quot;)
ropePart.FormFactor = 3
ropePart.Size = Vector3.new(1.75, 1.75, 1.75)
ropePart.Transparency = 0.8
ropePart.Shape = &quot;Ball&quot;
ropePart.Name = &quot;rope&quot;
ropePart.CanCollide = false

local ropePartMesh = Instance.new(&quot;SpecialMesh&quot;)
ropePartMesh.MeshType = &quot;FileMesh&quot;
ropePartMesh.Name = &quot;Mesh&quot;
ropePartMesh.MeshId = &quot;http://www.roblox.com/asset/?id=48423624&quot;
ropePartMesh.TextureId = &quot;http://www.roblox.com/asset/?id=48598142&quot;
ropePartMesh.Scale = Vector3.new(0.04, 0.04, 0.04)
ropePartMesh.Parent = ropePart

local fire = Instance.new(&quot;Fire&quot;)
fire.Size = 0.1
fire.Heat = 5.0
fire.Color = Color3.new(51/255, 102/255, 204/255)
fire.Parent = ropePart 

local float = Instance.new(&quot;BodyForce&quot;)
float.force = Vector3.new(0.0, ropePart:GetMass() * 196.1, 0.0)
float.Name = &quot;Floating&quot; 

function ropeCD()
&#9;local duration = 5
&#9;while duration &gt; 0 do 
&#9;&#9;if ropeTextRef then 
&#9;&#9;&#9;ropeTextRef.Text = duration 
&#9;&#9;&#9;ropeTextRef.Visible = true
&#9;&#9;end
&#9;&#9;duration = duration - 1
&#9;&#9;wait(1)
&#9;end
&#9;if ropeTextRef then ropeTextRef.Visible = false end 
&#9;rope = Tool:FindFirstChild(&quot;rope&quot;)
&#9;if rope then rope.Value = 0 end
&#9;if ropeBtnRef then ropeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59791591&quot; ropeBtnRef.Active = true end
end

function spikeCD()
&#9;local duration = 10 
&#9;while duration &gt; 0 do 
&#9;&#9;if myHumanoid then myHumanoid.WalkSpeed = 16.0 end&#9;
&#9;&#9;if spikeTextRef then 
&#9;&#9;&#9;spikeTextRef.Text = duration 
&#9;&#9;&#9;spikeTextRef.Visible = true 
&#9;&#9;end
&#9;&#9;duration = duration - 1
&#9;&#9;wait(1)
&#9;end&#9;
&#9;if spikeTextRef then spikeTextRef.Visible = false end 
&#9;spike = Tool:FindFirstChild(&quot;Spike&quot;)
&#9;if spike then spike.Value = 0 end 
&#9;if spikeBtnRef then spikeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59791072&quot; spikeBtnRef.Active = true end
end

function prisonCD()
&#9;local duration = 15
&#9;while duration &gt; 0 do 
&#9;&#9;if prisonTextRef then 
&#9;&#9;&#9;prisonTextRef.Text = duration
&#9;&#9;&#9;prisonTextRef.Visible = true
&#9;&#9;end
&#9;&#9;duration = duration - 1
&#9;&#9;wait(1)
&#9;end
&#9;if prisonTextRef then prisonTextRef.Visible = false end
&#9;prison = Tool:FindFirstChild(&quot;prison&quot;)
&#9;if prison then prison.Value = 0 end
&#9;if prisonBtnRef then prisonBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59791781&quot; prisonBtnRef.Active = true end 
end

function blow(parent, dmg,  hit)
&#9;if parent then 
&#9;&#9;if hit.Parent then 
&#9;&#9;&#9;humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;end
&#9;&#9;vCharacter = Tool.Parent
&#9;&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;&#9;if humanoid ~= nil and myHumanoid ~= humanoid and myHumanoid ~= nil then 
&#9;&#9;&#9;tagHumanoid(humanoid, vPlayer)
&#9;&#9;&#9;humanoid:TakeDamage(dmg)
&#9;&#9;end&#9;&#9;&#9;
&#9;end
end

function tagHumanoid(humanoid, player)
&#9;if humanoid ~= nil then
&#9;&#9;creator = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;creator.Name = &quot;creator&quot;
&#9;&#9;creator.Value = player
&#9;&#9;creator.Parent = humanoid
&#9;&#9;debris:AddItem(creator, 1)
&#9;end
end

function firefall()&#9;
&#9;local offset = 4.0
&#9;for i = 1, 12 do
&#9;&#9;fireballs[i] = ropePart:Clone()
&#9;&#9;fireballs[i].Parent = game.Workspace          &#9;&#9;
&#9;&#9;if i &lt; 4 then
&#9;&#9;&#9;fireballs[i].Position = myTorso.Position + Vector3.new(i * offset, 20.0, 0.0)
&#9;&#9;elseif i &lt; 7 then
&#9;&#9;&#9;fireballs[i].Position = myTorso.Position + Vector3.new(0.0, 20.0, (i - 3)* offset)
&#9;&#9;elseif i &lt; 10 then
&#9;&#9;&#9;fireballs[i].Position = myTorso.Position + Vector3.new(0.0, 20.0, (i-6) * -offset)
&#9;&#9;else
&#9;&#9;&#9;fireballs[i].Position = myTorso.Position + Vector3.new((i-9) * -offset, 20.0, 0.0)
&#9;&#9;end
&#9;&#9;local floatForce = Instance.new(&quot;BodyForce&quot;)
&#9;&#9;floatForce.force = Vector3.new(0, fireballs[i]:GetMass() * 100.0, 0) 
&#9;&#9;floatForce.Parent = fireballs[i]

&#9;&#9;debris:addItem(fireballs[i], 3.5)          
&#9;&#9;fireballs[i].Anchored = true
&#9;&#9;fireballs[i].Touched:connect(function(hit) blow(fireballs[i], ropeDamage, hit) end)&#9;&#9;
&#9;end
&#9;wait(0.3)
&#9;for i = 1, #fireballs do 
&#9;&#9;fireballs[i].Anchored = false 
&#9;end
end

function shoot()&#9;
&#9;vCharacter = Tool.Parent
&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;if vCharacter == nil or vPlayer == nil then
&#9;&#9;return
&#9;end

&#9;for i = 1, 12 do
&#9;&#9;local pos = CFrame.Angles(0, i * math.pi/6, 0) * myTorso.CFrame.lookVector

&#9;&#9;local spawnPos = myTorso.Position
&#9;&#9;spawnPos = spawnPos + (pos * 5.0)
&#9;&#9;
&#9;&#9;spikeProjectile = spikePart:Clone()
&#9;&#9;spikeProjectile.Name = &quot;rope&quot;
&#9;&#9;spikeProjectile.CFrame = CFrame.new(spawnPos, spawnPos + pos) * CFrame.Angles(-math.pi/2, 0, 0)
&#9;&#9;local floatForce = float:Clone()
&#9;&#9;floatForce.force = Vector3.new(0, spikePart:GetMass() * 196.1, 0)
&#9;&#9;floatForce.Parent = spikeProjectile
&#9;&#9;spikeProjectile.Velocity = pos * 75.0
&#9;&#9;spikeProjectile.Parent = game.Workspace
&#9;&#9;spikeProjectile.Touched:connect(function(hit) blow(spikeProjectile, spikeDamage,  hit) end)
&#9;&#9;debris:addItem(spikeProjectile, 4)
&#9;end
end 

function onActivated()
&#9;if Tool.Enabled == false then 
&#9;&#9;return &#9;
&#9;end

&#9;Tool.Enabled = false 

&#9;vCharacter = Tool.Parent
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;if myTorso == nil or myHumanoid == nil then 
&#9;&#9;return
&#9;end

&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;if vPlayer == nil then 
&#9;&#9;return
&#9;end
&#9;
&#9;rope = Tool:FindFirstChild(&quot;Rope&quot;)
&#9;prison = Tool:FindFirstChild(&quot;Prison&quot;)
&#9;spike = Tool:FindFirstChild(&quot;Spike&quot;)

&#9;sgRef = vPlayer.PlayerGui:FindFirstChild(&quot;SpellGui&quot;)
&#9;frameRef = sgRef:FindFirstChild(&quot;SpellGuiFrame&quot;)

&#9;if rope.Value == 1 or spike.Value == 1 or prison.Value == 1 then&#9;&#9;&#9;
&#9;&#9;&#9;bookSound = Tool:FindFirstChild(&quot;BookSound&quot;)
&#9;&#9;&#9;if bookSound then bookSound:Play() end
&#9;&#9;&#9;leftArm = vCharacter[&quot;Left Arm&quot;]
&#9;&#9;&#9;if leftArm then
&#9;&#9;&#9;&#9;bookClone = Book:Clone() 
&#9;&#9;&#9;&#9;bookClone.Parent = vCharacter
&#9;&#9;&#9;&#9;spark = Instance.new(&quot;Sparkles&quot;)&#9;
&#9;&#9;&#9;&#9;spark.Color = Color3.new(139/255, 0, 139/255)
&#9;&#9;&#9;&#9;spark.Parent = bookClone &#9;
&#9;&#9;&#9;&#9;bookWeld = Instance.new(&quot;Weld&quot;)
&#9;&#9;&#9;&#9;bookWeld.Part0 = leftArm
&#9;&#9;&#9;&#9;bookWeld.Part1 = bookClone
&#9;&#9;&#9;&#9;bookWeld.Name = &quot;LeftGrip&quot;
&#9;&#9;&#9;&#9;bookWeld.Parent = leftArm&#9;&#9;
&#9;&#9;&#9;&#9;bookWeld.C1 = CFrame.new(Vector3.new(-1.2, 0.0, 0.0)) * CFrame.Angles(math.pi/2, 0.0, math.pi/2)&#9;
&#9;&#9;&#9;&#9;Book.Transparency = 1.0
&#9;&#9;&#9;end

&#9;&#9;if rope.Value == 1 and (ropeCo == nil or coroutine.status(ropeCo) == &apos;dead&apos;) then 
&#9;&#9;&#9;ropeCo = coroutine.create(ropeCD)
&#9;&#9;&#9;coroutine.resume(ropeCo)
&#9;&#9;&#9;if frameRef then 
&#9;&#9;&#9;&#9;ropeBtnRef = frameRef:FindFirstChild(&quot;rope&quot;) 
&#9;&#9;&#9;&#9;ropeTextRef = frameRef:FindFirstChild(&quot;OText&quot;)
&#9;&#9;&#9;&#9;if ropeBtnRef then ropeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59791874&quot; ropeBtnRef.Active = false end 
&#9;&#9;&#9;end
&#9;&#9;&#9;firefall() 
&#9;&#9;elseif prison.Value == 1 and (prisonCo == nil or coroutine.status(prisonCo) == &apos;dead&apos;) then
&#9;&#9;&#9;prisonCo = coroutine.create(prisonCD)
&#9;&#9;&#9;coroutine.resume(prisonCo)&#9;
&#9;&#9;&#9;if frameRef then 
&#9;&#9;&#9;&#9;prisonBtnRef = frameRef:FindFirstChild(&quot;prison&quot;) 
&#9;&#9;&#9;&#9;prisonTextRef = frameRef:FindFirstChild(&quot;RText&quot;)
&#9;&#9;&#9;&#9;if prisonBtnRef then prisonBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59791906&quot; prisonBtnRef.Active = false end 
&#9;&#9;&#9;end
&#9;&#9;&#9;players = game.Players:GetChildren() 
&#9;&#9;&#9;for i = 1, #players do 
&#9;&#9;&#9;&#9;if players[i] ~= vPlayer then 
&#9;&#9;&#9;&#9;&#9;tempChar = players[i].Character
&#9;&#9;&#9;&#9;&#9;if tempChar then 
&#9;&#9;&#9;&#9;&#9;&#9;tempHumanoid = tempChar:FindFirstChild(&quot;Humanoid&quot;) 
&#9;&#9;&#9;&#9;&#9;&#9;tempTorso = tempChar:FindFirstChild(&quot;Torso&quot;)&#9;
&#9;&#9;&#9;&#9;&#9;&#9;if tempHumanoid and tempTorso then 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;distance = (tempTorso.Position - myTorso.Position).magnitude 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;if distance &lt; 30.0 then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;if tempChar:FindFirstChild(&quot;PrisonScript&quot;) == nil then &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript = Tool.PrisonScript:Clone()
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript.Parent = tempChar
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;newScript.Disabled = false 
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;elseif spike.Value == 1 and (spikeCo == nil or coroutine.status(spikeCo) == &apos;dead&apos;) then&#9;&#9;&#9;&#9;
&#9;&#9;&#9;spikeCo = coroutine.create(spikeCD)
&#9;&#9;&#9;coroutine.resume(spikeCo)
&#9;&#9;&#9;if frameRef then
&#9;&#9;&#9;&#9;spikeBtnRef = frameRef:FindFirstChild(&quot;Spike&quot;)
&#9;&#9;&#9;&#9;spikeTextRef = frameRef:FindFirstChild(&quot;SText&quot;)
&#9;&#9;&#9;&#9;if spikeBtnRef then spikeBtnRef.Image = &quot;http://www.roblox.com/asset/?id=59802033&quot; spikeBtnRef.Active = false end &#9;&#9;&#9;
&#9;&#9;&#9;end&#9;&#9;&#9;
&#9;&#9;&#9;shoot() 
&#9;&#9;end
&#9;&#9;wait(1.0)
&#9;&#9;myHumanoid.WalkSpeed = 16.0
&#9;&#9;if spark then spark:Remove() end
&#9;&#9;if bookClone then bookClone:Remove() end
&#9;&#9;Book.Transparency = 0.0
&#9;end
&#9;Tool.Enabled = true
end

function onEquipped()
&#9;vCharacter = Tool.Parent
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
end

function onUnequipped()
&#9;if bookClone then bookClone:Remove() end
&#9;Book.Transparency = 0.0
&#9;if spark then spark:Remove() end
&#9;if bookSound then bookSound:Stop() end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
</ProtectedString>
