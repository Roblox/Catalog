				<string name="Name">GuiScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local Book = Tool.Handle

local rope 
local spike 
local prison

local spikeText

local sg

local vCharacter
local vPlayer

-- Animations
local bookAnim

function castSpell(key)&#9;
&#9;key = key:lower()
&#9;if key == &quot;q&quot; then 
&#9;&#9;ropeCast()
&#9;elseif key == &quot;e&quot; then
&#9;&#9;spikeCast()
&#9;elseif key == &quot;r&quot; then
&#9;&#9;prisonCast()
&#9;end
end

function ropeCast() &#9;
&#9;prison = Tool:FindFirstChild(&quot;Prison&quot;) 
&#9;rope = Tool:FindFirstChild(&quot;Rope&quot;)
&#9;spike = Tool:FindFirstChild(&quot;Spike&quot;)
&#9;if rope and rope.Value == 0 then 
&#9;&#9;rope.Value = 1 
