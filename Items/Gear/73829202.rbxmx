&#9;&#9;Parent = tmpScreenGui,
&#9;&#9;--
&#9;&#9;Create&apos;Frame&apos;{
&#9;&#9;&#9;Name = &apos;TextMover&apos;,
&#9;&#9;&#9;BackgroundTransparency = 1,
