				<ProtectedString name="Source">local Mesh = script.Parent.Mesh
local blink = false

while true do

	blink = not blink
	if blink then
		Mesh.TextureId = &quot;http://www.roblox.com/asset/?id=28512114&quot;
	else
		Mesh.TextureId = &quot;http://www.roblox.com/asset/?id=33378623&quot;
	end
	wait(2)

end
