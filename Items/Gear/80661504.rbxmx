					<ProtectedString name="Source">-- PEWPEW BOOSH BROWWWWW
-- LASER BEAMS!

local orb = script.Parent
local Tool = orb.Tool.Value
local vCharacter = Tool.Parent
local orbHealth = orb.Health.Value
local sword = Tool.Handle

orb.Touched:connect(function(hit)
&#9;print(&quot;TouchedA&quot;)
&#9;if hit == nil or (hit.Parent == nil) or hit.Anchored == true then return end
&#9;if Tool == nil or Tool.Parent == nil or hit:IsDescendantOf(Tool.Parent) then return end
&#9;print(&quot;TouchedB&quot;)
&#9;-- final check, make sure sword is in-hand
&#9;&#9;local right_arm = vCharacter:FindFirstChild(&quot;Right Arm&quot;)
&#9;&#9;if (right_arm ~= nil) then
&#9;&#9;&#9;print(&quot;TouchedC&quot;)
&#9;&#9;&#9;orbHealth = orbHealth - 1
&#9;&#9;&#9;local vec = hit.Position - orb.Position 
&#9;&#9;&#9;hit.Velocity = vec.unit * (hit.Velocity.magnitude + 10)
&#9;&#9;&#9;orb.Transparency = .6
&#9;&#9;&#9;if orbHealth == 0 then orb:Destroy() end
&#9;&#9;&#9;wait(.1)
&#9;&#9;&#9;if orb then orb.Transparency = .8 end
&#9;&#9;end
end)</ProtectedString>
