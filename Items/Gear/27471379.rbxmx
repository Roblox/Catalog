							placeFirstOpponentPosition(&quot;third&quot;,x,y)
						elseif opponentShipNum == 4 then
							placeFirstOpponentPosition(&quot;fourth&quot;,x,y)
						elseif opponentShipNum == 5 then
							placeFirstOpponentPosition(&quot;fifth&quot;,x,y)
						end

					end
				end

			end)

		end
