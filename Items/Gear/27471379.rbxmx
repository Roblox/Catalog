function hasWon(num)
	if num == 1 then

		local opHits = 0
		for i = 1,11 do
			for j = 1, 9 do
				if opponentShipBoard[i][j] == 2 then
					opHits = opHits + 1
				end
			end
		end
		
		if opHits &gt; 16 then
			return true
		else
			return false
		end

	elseif num == 2 then

		local hits = 0
		for i = 1,11 do
			for j = 1, 9 do
				if playerShipBoard[i][j] == 2 then
					hits = hits + 1
				end
			end
		end
		
		if hits &gt; 16 then
			return true
		else
			return false
		end

	end
end

