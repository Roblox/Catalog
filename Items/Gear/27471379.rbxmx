function setupPlayerShip(x,y,name,size)

	local pc = playerGui.MainFrame.BoardPiece:clone()
	if size == 5 then
		pc.Image = &quot;http://www.roblox.com/asset/?id=26729984&quot;
	elseif size == 4 then
