				<ProtectedString name="Source">local Tool = script.Parent
local MyMouse = nil

function OnEquipped(mouse)
&#9;if mouse == nil then return end
&#9;MyMouse = mouse
end

function OnUnequipped()
&#9;MyMouse = nil
end

Tool.Equipped:connect(OnEquipped)
Tool.Unequipped:connect(OnUnequipped)

while true do
&#9;if MyMouse then MyMouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot; end
&#9;while not Tool.Enabled do wait() end
&#9;
&#9;if MyMouse then MyMouse.Icon = &quot;rbxasset://textures\\GunWaitCursor.png&quot;&#9;end
&#9;while Tool.Enabled do wait() end
end
