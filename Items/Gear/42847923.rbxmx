				<ProtectedString name="Source">local Tool = script.Parent
local anim = nil
local swoosh = nil

enabled = true
function onButton1Down(mouse)
	if not enabled then
		return
	end

	enabled = false
	mouse.Icon = &quot;rbxasset://textures\\ArrowFarCursor.png&quot;
	--wait(1)
	if anim ~= nil then anim:Play() end
	wait(.75)
	if swoosh ~= nil then swoosh:Play() end
	wait(.5)
	if swoosh ~= nil then swoosh:Play() end
	wait(.25)
	if anim ~= nil then anim:Stop() end
	--wait(3.5)
	mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;

	--while not Tool.Enabled do wait(.01) end

	enabled = true

end

function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end
	if Tool.Parent:FindFirstChild(&quot;Humanoid&quot;) ~= nil then
		anim = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;):LoadAnimation(Tool.xswordfast3)
		swoosh = Tool.Handle.Fire
		mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
		mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	end
end

function onUnequippedLocal()
	if swoosh ~= nil then swoosh:Stop() end
	swoosh = nil

	if anim ~= nil then anim:Stop() end
	if anim ~= nil then anim:remove() end
	anim = nil
end

Tool.Unequipped:connect(onUnequippedLocal)
Tool.Equipped:connect(onEquippedLocal)
