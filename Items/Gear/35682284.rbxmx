local downX = 150--100
local downY = -100--(-100)


radius = 7

local firstRad = radiusImages[radius-3]
local aboveRad = radiusImages[radius-2]
local belowRad = radiusImages[radius-4]

for i = 1,numIter do
	stormX = stormX + stormXStep
